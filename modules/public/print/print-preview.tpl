<div class="modal-dialog dialog-container">
  <div class="modal-content">
    <div class="panel">
      <div class="panel-header">
        <h3><i class="fa fa-print"></i> <%- data.title || '打印预览' %></h3>
        <div class="control-btn">
          <a class="panel-close btn-close"><i class="fa fa-close"></i></a>
        </div>
      </div>
      <div class="panel-content">
        <iframe src="./modules/public/print/print-preview.html" id="windowPrint" style="border: none; width: 21cm; height: 500px; display: block; margin: 0 auto;">

        </iframe>
        <div class="dialog-options">
          <button class="btn btn-primary" id="btnPrint">打印</button>
        </div>
      </div>
    </div>
  </div>
</div>
<% for(var i = 0; i < list.length; i++) { %>
<tr data-id="<%- list[i].roleId %>">
    <td><%- (i + 1) %></td>
    <td title="<%- list[i].roleName %>"><%- list[i].roleName %></td>
    <td title="<%- list[i].roleDesc %>"><%- list[i].roleDesc %></td>
    <td title="<%- list[i].departmentName %>"><%- list[i].departmentName %></td>
</tr>
<% } %>
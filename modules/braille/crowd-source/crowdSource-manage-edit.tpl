<div class="modal-dialog dialog-container">
  <div class="modal-content">
    <div class="panel">
      <div class="panel-header border-bottom">
        <h3><i class="fa fa-user-o"></i>任务领用</h3>
        <div class="control-btn">
          <a class="panel-close btn-close"><i class="fa fa-close"></i></a>
        </div>
      </div>
      <div class="panel-content">
        <div class="form-horizontal" id="formReportEdit">
          <div class="row">
            <div class="col-xs-6">
              <div class="form-group">
                <label class="col-xs-4 control-label">图书名称</label>
                <div class="col-xs-8">
                  <p class="form-control-static"><%- info.names %></p>
                </div>
              </div>
            </div>
            <div class="col-xs-6">
              <div class="form-group">
                <label class="col-xs-4 control-label">任务阶段</label>
                <div class="col-xs-8">
                  <p class="form-control-static"><%- info.flowNames %></p>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-xs-12">
              <div class="form-group">
                <label class="col-xs-2 control-label">任务简介</label>
                <div class="col-xs-10">
                  <p class="form-control-static"><%- info.taskContents %></p>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="dialog-options">
              <button type="button" class="btn btn-primary" id="btnCrowdSourceSave">确认领取</button>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
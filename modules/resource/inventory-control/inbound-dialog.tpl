<!--资源库-库存管理-入库-->
<div class="modal-dialog dialog-container">
  <div class="modal-content">
    <div class="panel">
      <div class="panel-header">
        <h3><i class="fa fa-user-o"></i><%- title %></h3>
        <div class="control-btn">
          <a class="panel-close btn-close"><i class="fa fa-close"></i></a>
        </div>
      </div>
      <div class="panel-content">
        <div class="form-horizontal" id="formDiv">
          <div class="row">
            <div class="col-xs-6">
              <div class="form-group">
                <label class="col-xs-4 control-label">入库数量<span class="must">*</span></label>
                <div class="col-xs-8">
                  <input class="form-control" name="rukushuliang" value="<%- info.rukushuliang %>" placeholder="入库数量" pattern="^\d{0,8}$" data-fv-regexp-message="请输入正整数，最大8位" data-fv-notempty="true"
                         data-fv-notempty-message="请填写入库数量">
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-xs-12">
              <div class="form-group">
                <label class="col-xs-2 control-label">备注</label>
                <div class="col-xs-10">
                  <textarea class="form-control" name="beizhu" placeholder="备注（最大1024字）" maxlength="1024"><%- info.beizhu %></textarea>
                </div>
              </div>
            </div>
          </div>
          <div class="dialog-options">
            <button class="btn btn-default" id="btnSave">保存</button>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!--译校通读-校对或通读-->
<!---------------------------------------------------------------------------------------------编辑状态--------------------------------------------------------------------------------------------->
<% if(isEdit) { %>
<div class="form-horizontal">
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label"><%- info.flowName %>结果</label>
        <div class="col-xs-8">
          <p class="form-control-static">
            <% if(!info.handleState || info.handleState === '1') { %>
            <label class="radio-inline"><input type="radio" name="handleState" value="1" checked/>通过</label>
            <label class="radio-inline"><input type="radio" name="handleState" value="2"/>不通过</label>
            <% } else { %>
            <label class="radio-inline"><input type="radio" name="handleState" value="1"/>通过</label>
            <label class="radio-inline"><input type="radio" name="handleState" value="2" checked/>不通过</label>
            <% } %>
          </p>
        </div>
      </div>
    </div>
  </div>
  <div class="row"></div>
  <div class="row">
    <label><%- info.flowName %>正误表</label>
  </div>
  <div class="row">
    <table class="table table-striped table-hover">
      <thead>
      <tr>
        <th>序号</th>
        <th>册数</th>
        <th>页码</th>
        <th>行</th>
        <th>内容</th>
        <th>
          <button class="addTableRow btn btn-xs btn-default"><i class="fa fa-plus box"></i></button>
        </th>
      </tr>
      </thead>
      <tbody id="trueOrFalseTb">
      <% if(info.qualityRightWrongEntities) %>
      <% for(var i = 0; i < info.qualityRightWrongEntities.length; i++) { %>
      <tr>
        <td><%- (i + 1) %></td>
        <td>
          <select name="fascicule" class="form-control" data-fv-notempty="true" data-fv-notempty-message="请选择分册">
            <% if(info.bookFascicleEntities && info.bookFascicleEntities.length > 0) %>
            <% for(var k = 0; k < info.bookFascicleEntities.length; k++){ %>
            <% if(info.bookFascicleEntities[k].bookFascicleId === info.qualityRightWrongEntities[k].bookFascicleId){ %>
            <option value='<%- info.bookFascicleEntities[k].bookFascicleId %>' selected="selected"><%- info.bookFascicleEntities[k].bookFascicleBookName %></option>
            <% }else{ %>
            <option value='<%- info.bookFascicleEntities[k].bookFascicleId %>'><%- info.bookFascicleEntities[k].bookFascicleBookName %></option>
            <% } %>
            <% } %>
          </select>
        </td>
        <td><input type="text" class="form-control" value="<%- info.yema %>" name="pageNum" placeholder="页码" maxlength="10"/></td>
        <td><input type="text" class="form-control" value="<%- info.hang %>" name="rowNum" placeholder="行" maxlength="10"/></td>
        <td><input type="text" class="form-control" value="<%- info.neirong %>" name="wrongInfo" placeholder="内容" maxlength="128"/></td>
        <td>
          <button class="removeTableRow btn btn-xs btn-danger"><i class="fa fa-minus box"></i></button>
        </td>
      </tr>
      <% } %>
      <% } %>
      </tbody>
    </table>
  </div>
  <div class="row">
    <label>差错计算</label>
  </div>
  <div class="row">
    <table class="table table-striped table-hover">
      <thead>
      <tr>
        <th>序号</th>
        <th>册数</th>
        <th>差错数</th>
        <th>差错率</th>
      </tr>
      </thead>
      <tbody id="diffTb">
      </tbody>
    </table>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <div class="form-group">
        <label class="col-xs-2 control-label"><%- info.flowName %>意见</label>
        <div class="col-xs-10">
          <textarea class="form-control" name="bookDetailAuthorResult" placeholder="<%- info.flowName %>意见（最大256字）" maxlength="256"></textarea>
        </div>
      </div>
    </div>
  </div>
  <% if(info.userTaskEntities) { %>
  <% for(var i = 0; i < info.userTaskEntities.length; i++) { %>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">下一流程</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.userTaskEntities[i].flowName %></p>
          <input type="hidden" name="flowId" value="<%- info.userTaskEntities[i].flowId %>">
          <input type="hidden" name="flowName" value="<%- info.userTaskEntities[i].flowName %>">
          <input type="hidden" name="nextCategory" value="<%- info.userTaskEntities[i].category %>">
          <input type="hidden" name="deptIds" value="<%- info.userTaskEntities[i].deptIds %>">
          <input type="hidden" name="roleIds" value="<%- info.userTaskEntities[i].roleIds %>">
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <% if(info.userTaskEntities[i].roleIds) { %>
      <div class="form-group">
        <label class="col-xs-4 control-label">任务人</label>
        <div class="col-xs-8">
          <div class="input-group">
            <p class="form-control-static" data-value="<%- info.userTaskEntities[i].nextFlowTaskUserIds %>"><%- info.userTaskEntities[i].nextFlowTaskUserNames %></p>
          </div>
          <div class="help-block with-errors"></div>
        </div>
      </div>
      <% } %>
    </div>
  </div>
  <% } %>
  <% } %>
</div>
<!---------------------------------------------------------------------------------------------查看状态--------------------------------------------------------------------------------------------->
<% } else { %>
<div class="form-horizontal">
</div>
<% } %>
<!--审稿改稿-->
<% if(isEdit) { %>
<div class="form-horizontal" id="formEdit">
  <div class="row">
  	<div class="col-xs-12">
  		<div class="form-group">
  			<label class="col-xs-2 control-label">录排文件</label>
  			<div class="col-xs-10">
  				<table class="table table-striped">
  					<thead>
  						<tr>
  							<th style="width: 50px;">序号</th>
  							<th style="width: 60px;">分册书</th>
  							<th>附件<span class="must-td">*</span></th>
  							<th>页数<span class="must-td">*</span></th>
  							<th style="width: 75px;">下载</th>
  							<th style="width: 75px;">预览</th>
  						</tr>
  					</thead>
  					<tbody>
  						<% for(var i = 0; i < info.fascicleList.length; i++) { %>
  						<tr data-area="contrat">
  							<td>
  								<%- i+1 %>
  								<input type="hidden" name="bookFascicleId" value="<%- info.fascicleList[i].bookFascicleId %>" />
  							</td>
  							<td><%- info.fascicleList[i].bookFascicleBookName %></td>
  							<td class="form-group">
  								<div class="input-group with-clear">
										<input type="text" class="form-control" name="hBookFileName" title="<%- info.fascicleList[i].hBookFileName %>" value="<%- info.fascicleList[i].hBookFileName %>" data-value="<%- info.fascicleList[i].hBookFileAddress %>" placeholder="分册书" readonly data-fv-notempty="true" data-fv-notempty-message="请选择文件">
										<span class="input-group-btn">
                    	<button class="btn btn-clear" type="button" data-action="clear"><i class="fa fa-remove box"></i></button>
                      <button class="btn btn-default" type="button" data-open="public/file/file-upload" data-data='{"type": "*.doc, *.docx, *.pdf"}'><i class="fa fa-file-o"></i></button>
                    </span>
									</div>
  							</td>
  							<td class="form-group">
  								<input type="text" class="form-control" name="hTaskPageNum" value="<%- info.fascicleList[i].hTaskPageNum %>"maxlength="8" pattern="^[0-9]*$"  data-fv-regexp-message="请输入大于0的整数" data-fv-notempty="true" data-fv-notempty-message="请输入图书页数" />
  							</td>
  							<% if(!info.fascicleList[i].hBookFileAddress) { %>
  							<td><button data-down="down" class="btn btn-default btn-rounded btn-xs" disabled>下载</button></td>	
  							<td><button data-down="view" class="btn btn-default btn-rounded btn-xs" disabled>预览</button></td>	
  							<% } else { %>
  							<td><button data-down="down" class="btn btn-default btn-rounded btn-xs">下载</button></td>
  							<td><button data-down="view" class="btn btn-default btn-rounded btn-xs">预览</button></td>	
  							<% } %>
  						</tr>
  						<% } %>
  					</tbody>
  				</table>
  			</div>
  		</div>
  	</div>
  </div>
  <% if(info.userTaskEntities) { %>
  <% for(var i = 0; i < info.userTaskEntities.length; i++) { %>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">下一流程</label>
        <div class="col-xs-8">
          <% if(!info.userTaskEntities[i].deptAndRoles) { %>
          <p class="form-control-static"><%- info.userTaskEntities[i].flowName %></p>
          <% } else { %>
          <p class="form-control-static">
            <%- info.userTaskEntities[i].flowName %>（<%- info.userTaskEntities[i].deptAndRoles %>）
          </p>
          <% } %>
          <input type="hidden" name="flowId" value="<%- info.userTaskEntities[i].flowId %>">
          <input type="hidden" name="flowName" value="<%- info.userTaskEntities[i].flowName %>">
          <input type="hidden" name="nextCategory" value="<%- info.userTaskEntities[i].category %>">
          <input type="hidden" name="deptIds" value="<%- info.userTaskEntities[i].deptIds %>">
          <input type="hidden" name="roleIds" value="<%- info.userTaskEntities[i].roleIds %>">
          <input type="hidden" name="taskId" value="<%- info.taskId %>">
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <% if(info.userTaskEntities[i].roleIds) { %>
      <div class="form-group">
        <label class="col-xs-4 control-label">任务人<span class="must">*</span></label>
        <div class="col-xs-8">
          <div class="input-group with-clear">
            <input type="text" class="form-control" name="taskUsers" title="<%- info.userTaskEntities[i].nextFlowTaskUserNames %>"
                   value="<%- info.userTaskEntities[i].nextFlowTaskUserNames %>"
                   data-value="<%- info.userTaskEntities[i].nextFlowTaskUserIds %>" placeholder="任务人" readonly
                   data-data="{searchRoleId: '', searchDepartmentId: ''}" data-fv-notempty="true" data-fv-notempty-message="请选择下一任务人">
            <span class="input-group-btn">
              <button class="btn btn-clear" type="button" data-action="clear" ><i class="fa fa-remove box"></i></button>
              <button class="btn btn-default" type="button" data-open="public/user/select-users" data-data='{"roleIds": "<%- info.userTaskEntities[i].roleIds %>", "deptIds": "<%- info.userTaskEntities[i].deptIds %>"}'>
              	<i class="fa fa-user-o"></i></button>
            </span>
          </div>
        </div>
      </div>
      <% } %>
    </div>
  </div>
  <% } %>
  <% } %>
</div>
<% } else { %>
<div class="form-horizontal">
  <div class="row">
  	<div class="col-xs-12">
  		<div class="form-group">
  			<label class="col-xs-2 control-label">录排文件</label>
  			<div class="col-xs-10">
  				<table class="table table-striped">
  					<thead>
  						<tr>
  							<th style="width: 50px;">序号</th>
  							<th style="width: 60px;">分册书</th>
  							<th>附件</th>
  							<th>页数</th>
  							<th style="width: 75px;">下载</th>
  							<th style="width: 75px;">预览</th>
  						</tr>
  					</thead>
  					<tbody>
  						<% for(var i = 0; i < info.fascicleList.length; i++) { %>
  						<tr data-area="contrat">
  							<td><%- i+1 %></td>
  							<td><%- info.fascicleList[i].bookFascicleBookName %></td>
  							<td><%- info.fascicleList[i].hBookFileName %></td>
  							<td><%- info.fascicleList[i].hTaskPageNum %></td>
  							<td><button data-down="downView" class="btn btn-default btn-rounded btn-xs" data-address = "<%- info.fascicleList[i].hBookFileAddress %>">下载</button></td>
  							<td><button data-down="htmlView" class="btn btn-default btn-rounded btn-xs" data-address = "<%- info.fascicleList[i].hBookFileAddress %>">预览</button></td>
  						</tr>
  						<% } %>
  					</tbody>
  				</table>
  			</div>
  		</div>
  	</div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">提交人</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.submitUserName %></p>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">提交时间</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.submitDatetime %></p>
        </div>
      </div>
    </div>
  </div>
</div>
<% } %>
            // 删除行
            that.$container.on('click', '.removeTableRow', function () {
                $(this).parents('tr').remove();
                // 重新显示序号
                var orderNo = 1;
                that.$("#dataTBody").find("td[data-name='orderNo']").each(function () {
                    $(this).html(orderNo);
                    orderNo += 1;
                });
            });
            // 保存两位小数
            that.$container.on('change', '.digitalInput', function () {
                var el = $(this);
                var price = el.val();
                var bPrice = parseFloat(price).toFixed(2);
                if (isNaN(bPrice)) {
                    bPrice = '0.00';
                }
                el.val(bPrice);
            });
        }


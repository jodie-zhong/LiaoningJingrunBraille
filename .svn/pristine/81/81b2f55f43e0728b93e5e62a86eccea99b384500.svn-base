<div class="modal-dialog dialog-container">
	<div class="modal-content">
		<div class="panel">
			<div class="panel-header">
				<h3><i class="fa fa-user-o"></i>添加选题</h3>
				<div class="control-btn">
					<a class="panel-close btn-close"><i class="fa fa-close"></i></a>
				</div>
			</div>
			<div class="panel-content border-bottom">
				<div class="form-horizontal" id="formSearchTopic">
					<div class="row">
						<div class="col-xs-5">
							<div class="form-group">
								<label class="col-xs-4 control-label">上报名称</label>
								<div class="col-sm-8">
									<input class="form-control" type="text" name="searchBookTopicName" id="searchBookTopicName"  data-search="searchBookTopicName" />
								</div>
							</div>
						</div>
						<div class="col-xs-5">
							<div class="form-group">
								<label class="col-xs-4 control-label">上报部门</label>
								<div class="col-sm-8">
									<div class="input-group">
										<input type="text" class="form-control" data-search="searchDepartmentId" name="searchDepartmentId"
											    placeholder="上报部门" readonly />
										<span class="input-group-btn">
                    <button class="btn btn-default" type="button" data-open="public/department/select-department"
							data-data="{}"><i class="fa fa-sitemap"></i></button>
                  </span>
									</div>
								</div>
							</div>
						</div>
						</div>
					<div class="row">
						<div class="col-xs-5">
							<div class="form-group">
								<label class="col-xs-4 control-label">上报起始时间</label>
								<div class="col-sm-8">
										<!--<input style="width: auto" class="form-control input-group date-time col-sm-6" type="text" name="searchTopicDateStart"  data-search="searchTopicDateStart" />~-->
										<!--<input style="width: auto" class="form-control input-group date-time col-sm-6" type="text" name="searchTopicDateEnd"  data-search="searchTopicDateEnd" />-->
									<!--<div class="input-group">-->
										<input type="text" class="form-control date-time" name="searchTopicDateStart"  data-search="searchTopicDateStart">
									<!--</div>-->
								</div>
							</div>
						</div>
						<div class="col-xs-5">
							<div class="form-group">
								<label class="col-xs-4 control-label">上报结束时间</label>
								<div class="col-sm-8">
									<!--<input style="width: auto" class="form-control input-group date-time col-sm-6" type="text" name="searchTopicDateStart"  data-search="searchTopicDateStart" />~-->
									<!--<input style="width: auto" class="form-control input-group date-time col-sm-6" type="text" name="searchTopicDateEnd"  data-search="searchTopicDateEnd" />-->
									<!--<div class="input-group">-->
										<input type="text" class="form-control date-time" name="searchTopicDateEnd"  data-search="searchTopicDateEnd">
									<!--</div>-->
								</div>
							</div>
						</div>
						<div class="col-xs-2">
							<button class="btn btn-primary" id="btnSearchReport">查询</button>
						</div>
					</div>
				</div>
			</div>
			<div class="panel-content">
				<div class="table-10-static">
					<table class="table table-striped table-hover" data-multi="true" id="selectTopicReportTable">
						<thead>
							<tr>
								<th>序号</th>
								<th><i class="fa fa-square-o" data-action="check"></i></th>
								<th>上报名称</th>
								<th>上报部门</th>
								<th>上报选题数量</th>
								<th>上报时间</th>
								<th>详细信息</th>
							</tr>
						</thead>
						<tbody id="selectReportTable">
						</tbody>
					</table>
				</div>
				<div class="dialog-options">
					<button class="btn btn-primary" id="btnReportCheckSelect">添加</button>
				</div>
			</div>
		</div>
	</div>
</div>
<!--合同模板管理-->
<div class="row">
	<div class="col-xs-12">
		<div class="row">
			<div class="col-xs-12">
				<div class="panel">
					<div class="panel-header custom-panel-header">
						<h3>模板列表</h3>
						<div class="control-btn">
							<button class="btn btn-xs btn-success" id="btnTemplateAdd">新增</button>
							<button class="btn btn-xs btn-blue" id="btnTemplateModify" disabled>修改</button>
							<!--<button class="btn btn-xs btn-primary" id="btnTemplateAdd" disabled>查询</button>-->
							<button class="btn btn-xs btn-danger" id="btnTemplateDel" disabled>删除</button>
						</div>
					</div>
					<div class="panel-content border-bottom">
						<div class="form-horizontal" id="formSearchTemplate">
							<div class="row">
								<div class="col-xs-5">
									<div class="form-group">
										<label class="col-xs-4 control-label">模板名称</label>
										<div class="col-xs-8">
											<input type="text" class="form-control" data-search = "searchTemplateName" />
										</div>
									</div>
								</div>
								<div class="col-xs-2">
									<button class="btn btn-primary" id="btnSearchTemplate">查询</button>
								</div>
								<div class="col-xs-5"></div>
							</div>
						</div>
					</div>
					<div class="panel-content">
						<div class="table-10">
							<table class="table table-striped table-hover">
								<thead>
									<tr>
										<th>序号</th>
										<th>模板名称</th>
										<th>下载次数</th>
										<th>上传人</th>
										<th>上传时间</th>
										<th>修改人</th>
										<th>修改时间</th>
										<th>备注</th>
										<th></th>
									</tr>
								</thead>
								<tbody id="templateListTable"></tbody>
							</table>
						</div>
						<div class="clearfix" id="templateListPaging"></div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
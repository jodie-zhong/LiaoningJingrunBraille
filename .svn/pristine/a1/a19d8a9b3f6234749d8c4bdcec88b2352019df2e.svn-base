            <th class="w-100">选题编辑</th>
            <th class="w-100">操作</th>
          </tr>
          </thead>
        </table>
        <div>
          <table class="table table-striped table-nowrap">
            <tbody id="selectReportTable">
            </tbody>
          </table>
        </div>
        <div class="dialog-options">
          <button type="button" class="btn btn-primary" id="btnReportSure">确定</button>
        </div>
      </div>
    </div>
  </div>

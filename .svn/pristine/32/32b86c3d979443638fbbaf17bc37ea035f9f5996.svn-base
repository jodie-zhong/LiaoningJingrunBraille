<!-- 变更备案 -->
<div class="row">
	<!-- 左侧树 -->
	<div class="col-xs-3">
		<div class="panel">
			<div class="panel-header custom-panel-header">
				<h3><i class="fa fa-braille"> 盲文读物管理- 变更备案</i></h3>
			</div>
			<div class="panel-content panel-height-1">
				<div id="monthTree"></div>
			</div>
		</div>
	</div>
	<div class="col-xs-9">
		<div class="row">
			<div class="col-xs-12">
				<div class="panel">
					<div class="panel-header custom-panel-header">
						<h3>
							申请列表
							<span class="icheck-inline task-about" id="taskAboutArea"></span>
						</h3>
						<div class="control-btn">
							<button class="btn btn-xs btn-primary" id="btnChangeHandle" disabled>处理</button>
							<button class="btn btn-xs btn-primary" id="btnChangeBatch" disabled>批量处理</button>
							<button class="btn btn-xs btn-default" id="btnChangeCheck" disabled>查看</button>
						</div>
					</div>
					<div class="panel-content">
						<div class="table-10">
							<table class="table table-striped table-hover" data-multi="true" id="workListTable">
								<thead id="workListTHeader">
									<tr>
										<th>序号</th>
										<th><i class="fa fa-square-o" data-action="check"></i></th>
										<th>变更图书</th>
										<th>责任编辑</th>
										<th>书号</th>
										<th>变更项</th>
										<th>申请状态</th>
									</tr>
								</thead>
								<tbody id="workListTBody">

								</tbody>
							</table>
						</div>
						<div class="pagination-container" id="workListPaging">

						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-xs-12">
				<div class="panel">
					<div class="panel-header custom-panel-header">
						<h3>流程查看</h3>
					</div>
					<div class="panel-content">
						<div class="table-5-static">
							<table class="table table-striped table-hover">
								<thead>
									<tr>
										<th>序号</th>
										<th>状态</th>
										<th>负责人</th>
										<th>起始时间</th>
										<th>完成时间</th>
										<th>审核结果</th>
										<th>详细信息</th>
									</tr>
								</thead>
								<tbody id="taskWorkflowTable">

								</tbody>
							</table>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
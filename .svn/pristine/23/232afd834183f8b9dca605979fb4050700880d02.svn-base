<div class="modal-dialog dialog-container">
  <div class="modal-content">
    <div class="panel">
      <div class="panel-header border-bottom">
        <h3><i class="fa fa-user"></i>密码修改</h3>
      </div>
      <div class="control-btn">
        <a class="panel-close btn-close"><i class="fa fa-close"></i></a>
      </div>
      <div class="row" id="passwordForm">
        <div class="col-md-4 col-md-offset-4">
          <div class="account-wall">
            <i class="user-img icons-faces-users-03"></i>
            <form class="form-signin form-horizontal" role="form" id="formPersonEdit">
              <div class="form-group append-icon m-b-20">
                <input type="password" name="oldUserPwd" id="oldUserPwd" class="form-control form-white password" placeholder="原密码" required  data-required-error="请填写原密码"/>
                <div class="help-block with-errors"></div>
              </div>
              <div class="form-group append-icon m-b-20">
                <input type="password" name="userPwd" id="userPwd"  class="form-control form-white password" placeholder="新密码" required data-required-error="请填写新密码" >
                <div class="help-block with-errors"></div>
              </div>
              <div class="form-group append-icon m-b-20">
                <input type="password" name="newPasswordAgain" data-match="#userPwd" data-match-error="两次输入密码不一致"   id="newPasswordAgain"  class="form-control form-white password" placeholder="再次输入密码" required data-required-error="请再次填写新密码">
                <div class="help-block with-errors"></div>
              </div>
              <button type="button" id="submit-form" class="btn btn-primary" data-style="expand-left">提交</button>
            </form>
          </div>
        </div>
      </div>
     </div>
  </div>
</div>
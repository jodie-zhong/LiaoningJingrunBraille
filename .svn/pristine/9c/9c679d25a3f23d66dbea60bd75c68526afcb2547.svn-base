<!--部门负责人审核-->
<div class="form-horizontal">
    <div class="row">
        <div class="col-xs-6">
            <div class="form-group">
                <label class="col-xs-4 control-label">上报名称</label>
                <div class="col-xs-8">
                    <p class="form-control-static"><%- info.reportTopicName %></p>
                </div>
            </div>
        </div>
        <div class="col-xs-6">
            <div class="form-group">
                <label class="col-xs-4 control-label">上报部门</label>
                <div class="col-xs-8">
                    <p class="form-control-static"><%- info.reportTopicDepartmentName %></p>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-6">
            <div class="form-group">
                <label class="col-xs-4 control-label">上报人</label>
                <div class="col-xs-8">
                    <p class="form-control-static"><%- info.reportTopicUserName %></p>
                </div>
            </div>
        </div>
        <div class="col-xs-6">
            <div class="form-group">
                <label class="col-xs-4 control-label">上报时间</label>
                <div class="col-xs-8">
                    <p class="form-control-static"><%- info.reportTopicDatetime %></p>
                </div>
            </div>
        </div>
    </div>
</div>
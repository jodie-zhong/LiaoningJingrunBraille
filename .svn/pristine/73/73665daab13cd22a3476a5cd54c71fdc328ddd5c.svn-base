<div style="position: absolute; top: 0;right: 0;left: 0;bottom: 0; background: #000000; opacity: .5;"></div>
<div class="modal-dialog dialog-container" style="width: 600px;">
  <div class="modal-content">
    <div class="panel">
      <div class="panel-header border-bottom">
        <h3><i class="fa fa-list"></i> 流程详情</h3>
        <div class="control-btn">
          <a class="panel-close btn-close"><i class="fa fa-close"></i></a>
        </div>
      </div>
      <div class="panel-content dialog-resize">
        <% if(info.lastTask) { %>
        <div class="form-horizontal form-compact border-bottom" style="padding-bottom: 10px;">
          <h5 class="c-primary" style="font-weight: bold">上一流程：</h5>
          <div class="form-group">
            <label class="col-xs-3 control-label">流程名称</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.lastTask.flowName %></p>
            </div>
          </div>
          <div class="form-group">
            <label class="col-xs-3 control-label">处理人</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.lastTask.handleUserName %></p>
            </div>
          </div>
          <div class="form-group">
            <label class="col-xs-3 control-label">任务人</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.lastTask.taskUsersName %></p>
            </div>
          </div>
          <div class="form-group">
            <label class="col-xs-3 control-label">起始时间</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.lastTask.submitDatetime %></p>
            </div>
          </div>
          <div class="form-group">
            <label class="col-xs-3 control-label">完成时间</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.lastTask.handleDatetime %></p>
            </div>
          </div>
          <div class="form-group">
            <label class="col-xs-3 control-label">审核结果</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.lastTask.handleStateName %></p>
            </div>
          </div>
        </div>
        <% } %>
        <% if(info.currentTask) { %>
        <div class="form-horizontal form-compact border-bottom" style="padding-bottom: 10px;">
          <h5 class="c-primary" style="font-weight: bold">当前流程：</h5>
          <div class="form-group">
            <label class="col-xs-3 control-label">流程名称</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.currentTask.flowName %></p>
            </div>
          </div>
          <div class="form-group">
            <label class="col-xs-3 control-label">处理人</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.currentTask.handleUserName %></p>
            </div>
          </div>
          <div class="form-group">
            <label class="col-xs-3 control-label">任务人</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.currentTask.taskUsersName %></p>
            </div>
          </div>
          <div class="form-group">
            <label class="col-xs-3 control-label">起始时间</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.currentTask.submitDatetime %></p>
            </div>
          </div>
          <div class="form-group">
            <label class="col-xs-3 control-label">完成时间</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.currentTask.handleDatetime %></p>
            </div>
          </div>
          <div class="form-group">
            <label class="col-xs-3 control-label">审核结果</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.currentTask.handleStateName %></p>
            </div>
          </div>
        </div>
        <% } %>
        <% if(info.nextTask) { %>
        <div class="form-horizontal form-compact border-bottom" style="padding-bottom: 10px;">
          <h5 class="c-primary" style="font-weight: bold">下一流程：</h5>
          <div class="form-group">
            <label class="col-xs-3 control-label">流程名称</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.nextTask.flowName %></p>
            </div>
          </div>
          <div class="form-group">
            <label class="col-xs-3 control-label">处理人</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.nextTask.handleUserName %></p>
            </div>
          </div>
          <div class="form-group">
            <label class="col-xs-3 control-label">任务人</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.nextTask.taskUsersName %></p>
            </div>
          </div>
          <div class="form-group">
            <label class="col-xs-3 control-label">起始时间</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.nextTask.submitDatetime %></p>
            </div>
          </div>
          <div class="form-group">
            <label class="col-xs-3 control-label">完成时间</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.nextTask.handleDatetime %></p>
            </div>
          </div>
          <div class="form-group">
            <label class="col-xs-3 control-label">审核结果</label>
            <div class="col-xs-8">
              <p class="form-control-static"><%- info.nextTask.handleStateName %></p>
            </div>
          </div>
        </div>
        <% } %>
      </div>
    </div>
  </div>
</div>
<!--出版计划统计-->
<div class="row">
	<div class="col-xs-3">
		<!-- 左侧树 -->
		<div class="panel panel-tree-0">
			<div class="panel-header custom-panel-header">
				<h3 class="modules-name" title="总编室管理 - 出版计划统计"><i class="fa fa-sitemap"></i> 总编室管理 - 出版计划统计</h3>
			</div>
			<div class="panel-content">
				<div id="monthTree"></div>
			</div>
		</div>
	</div>
	<div class="col-xs-9">
		<div class="row">
			<div class="col-xs-12">
				<div class="panel panel-book-0">
					<div class="panel-header custom-panel-header">
						<h3>出版计划列表</h3>
						<div class="control-btn">
							<button class="btn btn-xs btn-success" id="btnPublishAdd">新增</button>
							<button class="btn btn-xs btn-blue" id="btnPublishChange">修改</button>
							<button class="btn btn-xs btn-blue" id="btnPublishBatchChange">批量修改</button>
							<button class="btn btn-xs btn-danger" id="btnPublishDel">删除</button>
							<button class="btn btn-xs btn-blue" id="btnPublishSubmit">提交</button>
							<button class="btn btn-xs btn-blue" id="btnPublishHandle">处理</button>
							<button class="btn btn-xs btn-default" id="btnPublishCheck">查看</button>
							<button class="btn btn-xs btn-primary" id="export">导出</button>
						</div>
					</div>
					<div class="panel-content">
						<div class="table-10 table-scroll">
							<table class="table table-striped table-hover table-nowrap" style="width: 2380px; overflow-y: auto;" data-multi="true" id="workListTable">
								<thead>
									<tr>
										<th width="50px">序号</th>
										<th width="40px"><i class="fa fa-square-o" data-action="check"></i></th>
										<th width="90px">上报年份</th>
										<th width="150px">书名</th>
										<th width="150px">副书名</th>
										<th width="150px">丛（套）书名</th>
										<th width="150px">重点选题种类</th>
										<th width="150px">中图一级分类</th>
										<th width="150px">中图二级分类</th>
										<th width="150px">主要文种类型</th>
										<th width="150px">主要文种</th>
										<th width="150px">出版方式</th>
										<th width="150px">合作出版者</th>
										<th width="150px">丛（套）书作者</th>
										<th width="150px">著作责任者</th>
										<th width="150px">著作责任者简介</th>
										<th width="150px">选题类型</th>
										<th width="150px">计划出版年份</th>
										<th width="150px">计划出版月份</th>
										<th width="150px">千字数</th>
										<th width="150px">图幅数</th>
										<th width="150px">是否出版电子出版物</th>
										<th width="150px">版次</th>
										<th width="150px">装帧</th>
										<th width="150px">内容提要（20-500个字符）</th>
										<th width="150px">选题计划类型</th>
										<th width="150px">重大选题类型</th>
										<th width="150px">引进版权</th>
										<th width="150px">责任编辑</th>
										<th width="150px">定价（元）</th>
										<th width="150px">备注</th>
										<th width="150px">上报单位</th>
										<th width="150px">本社选题号</th>
										<th width="150px">选题状态</th>
										<th width="150px">主题出版</th>
									</tr>
								</thead>
								<tbody id="workListTBody"></tbody>
							</table>
						</div>
						<div class="pagination-container" id="workListPaging">

						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-xs-12">
				<div class="panel panel-flow">
					<div class="panel-header custom-panel-header">
						<h3>流程查看</h3>
					</div>
					<div class="panel-content">
						<table class="table table-striped table-nowrap table-top">
							<thead>
								<tr>
									<th style="width: 50px;">序号</th>
									<th style="width: 20%;">状态</th>
									<th style="width: 20%;">负责人</th>
									<th style="width: 20%;">起始时间</th>
									<th style="width: 20%;">完成时间</th>
									<th style="width: 20%;">审核结果</th>
									<th style="width: 100px;">详细信息</th>
								</tr>
							</thead>
						</table>
						<div class="tbody-5-static">
							<table class="table table-striped table-nowrap">
								<tbody id="taskWorkflowTable"></tbody>
							</table>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
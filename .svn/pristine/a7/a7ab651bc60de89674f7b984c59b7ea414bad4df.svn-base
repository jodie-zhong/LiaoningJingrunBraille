<!--图书季报-添加书-->
<div class="modal-dialog dialog-container">
  <div class="modal-content">
    <div class="panel">
      <div class="panel-header">
        <h3><i class="fa fa-user-o"></i>添加</h3>
        <div class="control-btn">
          <a class="panel-close btn-close"><i class="fa fa-close"></i></a>
        </div>
      </div>
      <div class="panel-content">
        <div class="form-horizontal" id="formDiv">
          <div class="row">
            <div class="col-xs-12">
              <label class="col-xs-2 control-label">查询时间</label>
              <div class="col-xs-8">
                <input style="width: 45%;" type="text" class="form-control" data-time="searchDate" maxlength="10" id="searchTimeStart" placeholder="开始时间"/>
                <span style="margin: 0 5px 0 5px;">~</span>
                <input style="width: 45%;" type="text" class="form-control" data-time="searchDate" maxlength="10" id="searchTimeEnd" placeholder="结束时间"/>
              </div>
              <button class="btn btn-primary" id="btnSearch">查询</button>
            </div>
          </div>
          <div class="row">
            <div class="table-10" style="margin: 25px 25px 0 25px;">
              <table class="table table-striped table-nowrap table-hover" data-multi="true" id="dataTable">
                <thead>
                <tr>
                  <th style="width: 50px;">序号</th>
                  <th style="width: 50px;"><i class="fa fa-square-o" data-action="check"></i></th>
                  <th>图书名称</th>
                  <th>类型</th>
                </tr>
                </thead>
                <tbody id="dataTBody">
                <tr data-noselect="true">
                  <td colspan="4"><p class="empty-tips">没有符合指定条件的数据</p></td>
                </tr>
                </tbody>
              </table>
            </div>
            <div class="clearfix" id="listPaging">
            </div>
          </div>
          <div class="dialog-options">
            <button class="btn btn-primary" id="btnConfirm">确定</button>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<% for(var i = 0; i < list.length; i++) { %>
<tr data-id="<%- list[i].bookId %>">
  <td><%- (i + 1) %></td>
  <td><%- list[i].bookName %></td>
  <td><%- list[i].bookTypeName %></td>
  <td><%- list[i].bookRecommendTypeName %></td>
  <td><%- list[i].bookIsUrgentName %></td>
  <td><%- list[i].bookStateName %></td>
</tr>
<% } %>
<!-- 选题备案 -->
<div class="row">
  <div class="col-xs-3">
    <!-- 左侧树 -->
    <div class="panel">
      <div class="panel-header custom-panel-header">
        <h3><i class="fa fa-book"></i> 选题备案</h3>
      </div>
      <div class="panel-content panel-height-1">
        <div id="monthTree">

        </div>
      </div>
    </div>
  </div>
  <div class="col-xs-9">
    <div class="row">
      <div class="col-xs-12">
        <div class="panel">
          <div class="panel-header border-bottom">
            <h3>选题列表</h3>
            <div class="control-btn">
              <button class="btn btn-xs btn-success" id="btnTopicAdd">重大选题备案</button>
              <button class="btn btn-xs btn-success" id="btnTopicModify">引进选题备案</button>
              <button class="btn btn-xs btn-primary" id="btnPrincipalCheck" disabled>部门负责人审核</button>
              <button class="btn btn-xs btn-primary" id="btnChiefCheck" disabled>总编/副总编审批</button>
              <button class="btn btn-xs btn-primary" id="btnTopicHold" disabled>上报版署</button>
              <button class="btn btn-xs btn-primary" id="btnTopicHold" disabled>备案登记</button>
            </div>
          </div>
          <div class="panel-content border-bottom">
            <div class="form-horizontal" id="formSearchTask">
              <div class="row">
                <div class="col-xs-5">
                  <div class="form-group">
                    <label class="col-xs-4 control-label">筛选</label>
                    <div class="col-sm-8">
                      <select class="form-control">
                        <option>待我处理</option>
                        <option>与我相关</option>
                      </select>
                    </div>
                  </div>
                </div>
                <div class="col-xs-5">
                  <div class="form-group">
                    <label class="col-xs-4 control-label">选题名称</label>
                    <div class="col-sm-8">
                      <input class="form-control" data-search="searchTopicName">
                    </div>
                  </div>
                </div>
                <div class="col-xs-2">

                </div>
              </div>
              <div class="row">
                <div class="col-xs-5">
                  <div class="form-group">
                    <label class="col-xs-4 control-label">选题类别</label>
                    <div class="col-sm-8">
                      <select class="form-control" data-search="searchTopicType">
                        <option value="">全部</option>
                      </select>
                    </div>
                  </div>
                </div>
                <div class="col-xs-5">
                </div>
                <div class="col-xs-2">
                  <button class="btn btn-primary" id="btnSearchTask">搜索</button>
                </div>
              </div>
            </div>
          </div>
          <div class="panel-content">
            <div class="table-10">
              <table class="table table-striped table-hover" data-multi="true" id="taskListTableContainer">
                <thead id="taskListTableHeader">
                <tr>
                  <th>序号</th>
                  <th><i class="fa fa-square-o" data-action="check"></i></th>
                  <th>选题名称</th>
                  <th>选题类别</th>
                  <th>选题来源</th>
                  <th>是否加急</th>
                  <th>选题状态</th>
                </tr>
                </thead>
                <tbody id="taskListTable">

                </tbody>
              </table>
            </div>
            <div class="pagination-container" id="taskListPaging">

            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="panel">
          <div class="panel-header custom-panel-header">
            <h3>流程查看</h3>
          </div>
          <div class="panel-content">
            <div class="table-5-static">
              <table class="table table-striped table-hover">
                <thead>
                <tr>
                  <th>序号</th>
                  <th>状态</th>
                  <th>负责人</th>
                  <th>起始时间</th>
                  <th>完成时间</th>
                  <th>审核结果</th>
                  <th>详细信息</th>
                </tr>
                </thead>
                <tbody id="taskWorkflowTable">

                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
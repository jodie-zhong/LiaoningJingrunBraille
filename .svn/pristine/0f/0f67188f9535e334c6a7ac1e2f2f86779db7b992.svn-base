<!--成本预算-成本预算设置-->
<div class="modal-dialog dialog-container" style="width: 90%;">
  <div class="modal-content">
    <div class="panel">
      <div class="panel-header">
        <h3><i class="fa fa-user-o"></i>成本预算设置</h3>
        <div class="control-btn">
          <a class="panel-close btn-close"><i class="fa fa-close"></i></a>
        </div>
      </div>
      <div class="panel-content">
        <div class="form-horizontal form-compact" id="formDiv">
          <table class="table table-striped table-hover table-nowrap" data-multi="true">
            <thead>
            <tr>
              <th rowspan="2" style="width: 45px;">序号</th>
              <th rowspan="2" title="档位名称">档位名称</th>
              <th colspan="3" title="组稿数(元/种)">组稿数(元/种)</th>
              <th colspan="3" title="一审(元/千字)">一审(元/千字)</th>
              <th colspan="3" title="二审(元/千字)">二审(元/千字)</th>
              <th rowspan="2" title="三审(元/千字)">三审(元/千字)</th>
              <th rowspan="2" title="质检(元/千字)">质检(元/千字)</th>
              <th rowspan="2" title="封面设计(元/种)">封面设计(元/种)</th>
              <th rowspan="2" title="作者稿酬">作者稿酬</th>
              <th rowspan="2" title="编务费(元/种)">编务费(元/种)</th>
              <th rowspan="2" style="width: 45px;">
                <button class="addTableRow btn btn-xs btn-default" style="margin-right: 0;"><i class="fa fa-plus box"></i></button>
              </th>
            </tr>
            <tr>
              <th>社组选</th>
              <th>市场书</th>
              <th>大字本</th>
              <th>甲</th>
              <th>乙</th>
              <th>丙</th>
              <th>甲</th>
              <th>乙</th>
              <th>丙</th>
            </tr>
            </thead>
            <tbody id="dataTBody">
            <% if(info.rightWrongList) { %>
            <% for(var i = 0; i < info.rightWrongList.length; i++) { %>
            <tr>
              <td data-name="orderNo"><%- (i + 1) %></td>
              <td>
                <div class="form-group">
                  <div class="col-xs-12">
                    <input class="form-control" value="<%- info.rightWrongList[i].pm1 %>" name="pm1" placeholder="档位名称" maxlength="32" data-fv-notempty="true" data-fv-notempty-message="请填写档位名称"/>
                  </div>
                </div>
              </td>
              <td>
                <div class="form-group">
                  <div class="col-xs-12">
                    <input class="form-control" value="<%- info.rightWrongList[i].pm2 %>" name="pm2" placeholder="社组选" maxlength="3" data-fv-notempty="true" data-fv-notempty-message="请填写预算"
                           pattern="^(([0-9]*\.[0-9]{1,2})|([0-9]*[1-9][0-9]*\.[0-9]{1,2})|([0-9]*[1-9][0-9]*))$" data-fv-regexp-message="请输入大于0的数字"/>
                  </div>
                </div>
              </td>
              <td>
                <div class="form-group">
                  <div class="col-xs-12">
                    <input class="form-control" value="<%- info.rightWrongList[i].pm3 %>" name="pm3" placeholder="市场书" maxlength="3" data-fv-notempty="true" data-fv-notempty-message="请填写预算"
                           pattern="^(([0-9]*\.[0-9]{1,2})|([0-9]*[1-9][0-9]*\.[0-9]{1,2})|([0-9]*[1-9][0-9]*))$" data-fv-regexp-message="请输入大于0的数字"/>
                  </div>
                </div>
              </td>
              <td>
                <div class="form-group">
                  <div class="col-xs-12">
                    <input class="form-control" value="<%- info.rightWrongList[i].pm4 %>" name="pm4" placeholder="大字本" maxlength="3" data-fv-notempty="true" data-fv-notempty-message="请填写预算"
                           pattern="^(([0-9]*\.[0-9]{1,2})|([0-9]*[1-9][0-9]*\.[0-9]{1,2})|([0-9]*[1-9][0-9]*))$" data-fv-regexp-message="请输入大于0的数字"/>
                  </div>
                </div>
              </td>
              <td>
                <div class="form-group">
                  <div class="col-xs-12">
                    <input class="form-control" value="<%- info.rightWrongList[i].pm5 %>" name="pm5" placeholder="甲" maxlength="3" data-fv-notempty="true" data-fv-notempty-message="请填写预算"
                           pattern="^(([0-9]*\.[0-9]{1,2})|([0-9]*[1-9][0-9]*\.[0-9]{1,2})|([0-9]*[1-9][0-9]*))$" data-fv-regexp-message="请输入大于0的数字"/>
                  </div>
                </div>
              </td>
              <td>
                <div class="form-group">
                  <div class="col-xs-12">
                    <input class="form-control" value="<%- info.rightWrongList[i].pm6 %>" name="pm6" placeholder="乙" maxlength="3" data-fv-notempty="true" data-fv-notempty-message="请填写预算"
                           pattern="^(([0-9]*\.[0-9]{1,2})|([0-9]*[1-9][0-9]*\.[0-9]{1,2})|([0-9]*[1-9][0-9]*))$" data-fv-regexp-message="请输入大于0的数字"/>
                  </div>
                </div>
              </td>
              <td>
                <div class="form-group">
                  <div class="col-xs-12">
                    <input class="form-control" value="<%- info.rightWrongList[i].pm7 %>" name="pm7" placeholder="丙" maxlength="3" data-fv-notempty="true" data-fv-notempty-message="请填写预算"
                           pattern="^(([0-9]*\.[0-9]{1,2})|([0-9]*[1-9][0-9]*\.[0-9]{1,2})|([0-9]*[1-9][0-9]*))$" data-fv-regexp-message="请输入大于0的数字"/>
                  </div>
                </div>
              </td>
              <td>
                <div class="form-group">
                  <div class="col-xs-12">
                    <input class="form-control" value="<%- info.rightWrongList[i].pm8 %>" name="pm8" placeholder="甲" maxlength="3" data-fv-notempty="true" data-fv-notempty-message="请填写预算"
                           pattern="^(([0-9]*\.[0-9]{1,2})|([0-9]*[1-9][0-9]*\.[0-9]{1,2})|([0-9]*[1-9][0-9]*))$" data-fv-regexp-message="请输入大于0的数字"/>
                  </div>
                </div>
              </td>
              <td>
                <div class="form-group">
                  <div class="col-xs-12">
                    <input class="form-control" value="<%- info.rightWrongList[i].pm9 %>" name="pm9" placeholder="乙" maxlength="3" data-fv-notempty="true" data-fv-notempty-message="请填写预算"

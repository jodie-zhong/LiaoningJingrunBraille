<!-- 选题征集 -->
<div class="row">
  <div class="col-xs-3">
    <!-- 左侧树 -->
    <div class="panel">
      <div class="panel-header custom-panel-header">
        <h3><i class="fa fa-book"></i> 选题管理</h3>
      </div>
      <div class="panel-content panel-height-1">
        <div id="monthTree">

        </div>
      </div>
    </div>
  </div>
  <div class="col-xs-9">
    <div class="row">
      <div class="col-xs-12">
        <div class="panel">
          <div class="panel-header custom-panel-header">
            <h3>选题列表
              <span class="icheck-inline task-about" id="taskAboutArea"></span>
            </h3>
            <div class="control-btn">
              <button class="btn btn-xs btn-success" id="btnTopicAdd" data-auth="0211">新增</button>
              <button class="btn btn-xs btn-blue" id="btnTopicModify" data-auth="0212" disabled>修改</button>
              <button class="btn btn-xs btn-danger" id="btnTopicDelete" data-auth="0213" disabled>删除</button>
              <button class="btn btn-xs btn-blue" id="btnTopicSubmit" data-auth="0214" disabled>申报</button>
              <button class="btn btn-xs btn-blue" id="btnTopicHandle" data-auth="0216" disabled>处理</button>
              <button class="btn btn-xs btn-blue" id="btnTopicBatch" data-auth="0217" disabled>批量处理</button>
              <button class="btn btn-xs btn-default" id="btnTopicView" data-auth="0215" disabled>查看</button>
            </div>
          </div>
          <div class="panel-content">
            <div class="table-10">
              <table class="table table-striped table-hover" data-multi="true" id="workListTable">
                <thead>
                <tr>
                  <th>序号</th>
                  <th><i class="fa fa-square-o" data-action="check"></i></th>
                  <th>选题名称</th>
                  <th>选题类别</th>
                  <th>选题来源</th>
                  <th>是否加急</th>
                  <th>处理人</th>
                  <th>选题状态</th>
                </tr>
                </thead>
                <tbody id="workListTBody">

                </tbody>
              </table>
            </div>
            <div class="pagination-container" id="workListPaging">

            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="panel">
          <div class="panel-header custom-panel-header">
            <h3>流程查看</h3>
          </div>
          <div class="panel-content">
            <div class="table-5-static">
              <table class="table table-striped table-hover">
                <thead>
                <tr>
                  <th>序号</th>
                  <th>状态</th>
                  <th>负责人</th>
                  <th>起始时间</th>
                  <th>完成时间</th>
                  <th>审核结果</th>
                  <th>详细信息</th>
                </tr>
                </thead>
                <tbody id="taskWorkflowTable">

                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<% if(isEdit) { %>
<div class="form-horizontal">
  <div class="row">
    <div class="col-xs-12">
      <div class="form-group ">
        <label class="col-xs-2 control-label">确认结果<span class="must">*</span></label>
        <div class="col-xs-10">
          <label class="radio-inline">
            <% if(!info.handleState || info.handleState === '1') { %>
            <input type="radio" name="handleState" value="1" data-fv-notempty="true" data-fv-notempty-message="请选择确认结果" checked/>放弃选题
            <% } else { %>
            <input type="radio" name="handleState" value="1" data-fv-notempty="true" data-fv-notempty-message="请选择确认结果"/>放弃选题
            <% } %>
          </label>
          <label class="radio-inline">
            <% if(info.handleState === '2') { %>
            <input type="radio" name="handleState" value="2" data-fv-notempty="true" data-fv-notempty-message="请选择确认结果" checked/>修改再上报
            <% } else { %>
            <input type="radio" name="handleState" value="2" data-fv-notempty="true" data-fv-notempty-message="请选择确认结果"/>修改再上报
            <% } %>
          </label>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <div class="form-group">
        <label class="col-xs-2 control-label">备注</label>
        <div class="col-xs-10">
          <textarea class="form-control" name="handleContentOne" maxlength="2048"><%- info.handleContentOne %></textarea>
        </div>
      </div>
    </div>
  </div>
  <div data-area="edit" style="display: none;">
    <div class="row">
      <div class="col-xs-6">
        <div class="form-group">
          <label class="col-xs-4 control-label">选题名称<span class="must">*</span></label>
          <div class="col-xs-8">
            <div class="input-group">
              <input type="text" class="form-control" name="bookTopicName" maxlength="128" value="<%- info.bookTopicName %>" data-fv-notempty="true" data-fv-notempty-message="请输入选题名称"/>
              <span class="input-group-btn">
                            <button class="btn btn-default" type="button" id="btnTopicSimilar">查重</button>
                    </span>
            </div>
          </div>
        </div>
      </div>
      <div class="col-xs-6">
        <div class="form-group">
          <label class="col-xs-4 control-label">丛书名称</label>
          <div class="col-xs-8">
            <input type="text" class="form-control" value="<%- info.bookSeriesName %>" maxlength="128" name="bookSeriesName"/>
          </div>
        </div>
      </div>

    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="form-group">
          <label class="col-xs-2 control-label">选题类型<span class="must">*</span></label>
          <div class="col-xs-10">
            <label class="checkbox-inline">
              <% if(info.bookTopicType && info.bookTopicType.indexOf('1') >= 0) { %>
              <input type="checkbox" name="bookTopicType" value="1" data-fv-choice="true" data-fv-choice-min="1"
            data-fv-choice-max="3" data-fv-choice-message="请选择1~3个类型"  checked/>盲文读物
              <% } else { %>
              <input type="checkbox" name="bookTopicType" value="1"/>盲文读物
              <% } %>
            </label>
            <label class="checkbox-inline">
              <% if(info.bookTopicType && info.bookTopicType.indexOf('2') >= 0) { %>
              <input type="checkbox" name="bookTopicType" value="2" checked/>大字本读物
              <% } else { %>
              <input type="checkbox" name="bookTopicType" value="2"/>大字本读物
              <% } %>
            </label>
            <label class="checkbox-inline">
              <% if(info.bookTopicType && info.bookTopicType.indexOf('3') >= 0) { %>
              <input type="checkbox" name="bookTopicType" value="3" checked/>普通读物
              <% } else { %>
              <input type="checkbox" name="bookTopicType" value="3" data-fv-choice="true" data-fv-choice-min="1"
            data-fv-choice-max="3" data-fv-choice-message="请选择1~3个类型"/>普通读物
              <% } %>
            </label>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="form-group ">
          <label class="col-xs-2 control-label">推荐类型<span class="must">*</span></label>
          <div class="col-xs-10">
            <label class="radio-inline">
              <% if(!info.bookTopicRecommendType || info.bookTopicRecommendType === '2') { %>
              <input type="radio" name="bookTopicRecommendType" value="2" data-fv-notempty="true" data-fv-notempty-message="请选择推荐类型" checked/>非社推荐选题
              <% } else { %>
              <input type="radio" name="bookTopicRecommendType" value="2" data-fv-notempty="true" data-fv-notempty-message="请选择推荐类型"/>非社推荐选题
              <% } %>
            </label>
            <label class="radio-inline">
              <% if(info.bookTopicRecommendType && info.bookTopicRecommendType === '1') { %>
              <input type="radio" name="bookTopicRecommendType" value="1" data-fv-notempty="true" data-fv-notempty-message="请选择推荐类型" checked/>社推荐选题
              <% } else { %>
              <input type="radio" name="bookTopicRecommendType" value="1" data-fv-notempty="true" data-fv-notempty-message="请选择推荐类型"/>社推荐选题
              <% } %>
            </label>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="form-group ">
          <label class="col-xs-2 control-label">图书类别</label>
          <div class="col-xs-3">
            <select name="bookTopicOneKind" class="form-control" id="bookTopicOne" data-fv-notempty="true" data-fv-notempty-message="请选择图书类别">
            </select>
          </div>
          <div class="col-xs-3">
            <select name="bookTopicTwoKind" class="form-control" id="bookTopicTwo" data-fv-notempty="true" data-fv-notempty-message="请选择图书类别">
            </select>
          </div>
        </div>
      </div>

    </div>
    <div class="row">
      <div class="col-xs-6">
        <div class="form-group">
          <label class="col-xs-4 control-label">原作者</label>
          <div class="col-xs-8">
            <input type="text" class="form-control" maxlength="32" value="<%- info.bookDetailAuthor %>" name="bookDetailAuthor"/>
          </div>
        </div>
      </div>
      <div class="col-xs-6">
        <div class="form-group">
          <label class="col-xs-4 control-label">著作方式</label>
          <div class="col-xs-8">
            <input type="text" class="form-control" maxlength="64" value="<%- info.bookDetailWorkWay %>" name="bookDetailWorkWay"/>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-6">
        <div class="form-group">
          <label class="col-xs-4 control-label">学历</label>
          <div class="col-xs-8">
            <input type="text" class="form-control" maxlength="32" value="<%- info.bookDetailAuthorEducation %>"
                   name="bookDetailAuthorEducation"/>
          </div>
        </div>
      </div>
      <div class="col-xs-6">
        <div class="form-group ">
          <label class="col-xs-4 control-label">预计字数</label>
          <div class="col-xs-8">
            <input type="number" class="form-control" maxlength="10" value="<%- info.bookDetailWords %>" name="bookDetailWords"/>
          </div>
        </div>
      </div>

    </div>
    <div class="row">
      <div class="col-xs-6">
        <div class="form-group">
          <label class="col-xs-4 control-label">职称</label>
          <div class="col-xs-8">
            <input type="text" class="form-control" maxlength="32" value="<%- info.bookDetailAuthorTitles %>"
                   name="bookDetailAuthorTitles"/>
          </div>
        </div>
      </div>
      <div class="col-xs-6">
        <div class="form-group ">
          <label class="col-xs-4 control-label">交稿时间</label>
          <div class="col-xs-8">
            <input type="text" class="form-control date-time" maxlength="20" value="<%- info.bookDetailSubmitDatetime %>"
                   name="bookDetailSubmitDatetime"/>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="form-group ">
          <label class="col-xs-2 control-label">专长</label>
          <div class="col-xs-10">
            <input class="form-control" maxlength="32" value="<%- info.bookDetailAuthorSpeciality %>"
                   name="bookDetailAuthorSpeciality">
          </div>
        </div>
      </div>

    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="form-group">
          <label class="col-xs-2 control-label">著作及科研成果</label>
          <div class="col-xs-10">
            <textarea class="form-control" maxlength="256" name="bookDetailAuthorResult"><%- info.bookDetailAuthorResult %></textarea>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="form-group">
          <label class="col-xs-2 control-label">工作单位及联系方式</label>
          <div class="col-xs-10">
        <textarea class="form-control" maxlength="128" name="bookDetailAuthorContactWay"><%- info.bookDetailAuthorContactWay %></textarea>
          </div>
        </div>
      </div>

    </div>
    <div class="row">
      <div class="col-xs-6">
        <div class="form-group">
          <label class="col-xs-4 control-label">原书名称<span class="must">*</span></label>
          <div class="col-xs-8">
            <input type="text" class="form-control" value="<%- info.bookDetailName %>" name="bookDetailName" maxlength="128" data-fv-notempty="true" data-fv-notempty-message="请输入原书名称"/>
          </div>
        </div>
      </div>
      <div class="col-xs-6">
        <div class="form-group">
          <label class="col-xs-4 control-label">原书版次<span class="must">*</span></label>
          <div class="col-xs-8">
            <input type="number" class="form-control" value="<%- info.bookDetailEdition %>" name="bookDetailEdition" maxlength="128" data-fv-notempty="true" data-fv-notempty-message="请输入原书版次"/>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-6">
        <div class="form-group">
          <label class="col-xs-4 control-label">原出版社<span class="must">*</span></label>
          <div class="col-xs-8">
            <input type="text" class="form-control" value="<%- info.bookDetailPublishingHouse %>" name="bookDetailPublishingHouse" maxlength="128" data-fv-notempty="true" data-fv-notempty-message="请输入原出版社"/>
          </div>
        </div>
      </div>
      <div class="col-xs-6">
        <div class="form-group">
          <label class="col-xs-4 control-label">原书出版时间<span class="must">*</span></label>
          <div class="col-xs-8">
            <input type="text" class="form-control date-time" value="<%- info.bookDetailPublicationDate %>"
                   name="bookDetailPublicationDate" maxlength="32"/>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="form-group">
          <label class="col-xs-2 control-label">内容简述及特色</label>
          <div class="col-xs-10">
            <textarea class="form-control" name="bookDetailSketched" maxlength="1024"><%- info.bookDetailSketched %></textarea>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="form-group">
          <label class="col-xs-2 control-label">市场调研情况</label>
          <div class="col-xs-10">
            <textarea class="form-control" name="bookDetailMarketMsg" maxlength="1024"><%- info.bookDetailMarketMsg %></textarea>
          </div>
        </div>
      </div>
    </div>
  </div>
  <% if(info.userTaskEntities) { %>
  <% for(var i = 0; i < info.userTaskEntities.length; i++) { %>
  <div class="row" data-flow="<%- info.userTaskEntities[i].flowId %>" data-area="nextFlow" style="display: none;">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">下一流程<span class="must">*</span></label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.userTaskEntities[i].flowName %></p>
          <input type="hidden" name="flowId" value="<%- info.userTaskEntities[i].flowId %>">
          <input type="hidden" name="flowName" value="<%- info.userTaskEntities[i].flowName %>">
          <input type="hidden" name="nextCategory" value="<%- info.userTaskEntities[i].category %>">
          <input type="hidden" name="deptIds" value="<%- info.userTaskEntities[i].deptIds %>">
          <input type="hidden" name="roleIds" value="<%- info.userTaskEntities[i].roleIds %>">
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <% if(info.userTaskEntities[i].roleIds) { %>
      <div class="form-group">
        <label class="col-xs-4 control-label">任务人<span class="must">*</span></label>
        <div class="col-xs-8">
          <div class="input-group">
            <input type="text" class="form-control" name="taskUsers"
                   value="<%- info.userTaskEntities[i].nextFlowTaskUserNames %>"
                   data-value="<%- info.userTaskEntities[i].nextFlowTaskUserIds %>" placeholder="任务人" readonly
                   data-data="{searchRoleId: '', searchDepartmentId: ''}" data-fv-notempty="true" data-fv-notempty-message="请选择下一任务人">
            <span class="input-group-btn">
              <button class="btn btn-default" type="button" data-open="public/user/select-users" data-data="{}"><i
                      class="fa fa-user-o"></i></button>
            </span>
          </div>
        </div>
      </div>
      <% } %>
    </div>
  </div>
  <% } %>
  <% } %>
</div>
<% } else { %>
<div class="form-horizontal">
  <div class="row">
    <div class="col-xs-12">
      <div class="form-group ">
        <label class="col-xs-2 control-label">确认结果</label>
        <div class="col-xs-10">
          <p class="form-control-static">
            <% if(!info.handleState || info.handleState === '1') { %>
            放弃选题
            <% } %>
            <% if(info.handleState === '2') { %>
            修改再上报
            <% } %>
          </p>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <div class="form-group">
        <label class="col-xs-2 control-label">备注</label>
        <div class="col-xs-10">
          <pre class="form-control-static"><%- info.handleContentOne %></pre>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">提交人</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.submitUserName %></p>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">提交时间</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.submitDatetime %></p>
        </div>
      </div>
    </div>
  </div>
</div>
<% } %>
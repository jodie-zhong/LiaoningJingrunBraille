<!--备案登记(收集文件)-->
<% if(isEdit) { %>
<div class="form-horizontal" id="formEdit">
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">图书名称:</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.bookName %></p>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">责任编辑:</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.dutyEditor %></p>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">发稿单:</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.bookName %>发稿单</p>
          <button class="btn btn-xs btn-default printManuscript"
                  data-download-address="<%- info.bookFileAddress %>">打印
          </button>
          <button class="btn btn-xs btn-default downloadManuscript">下载</button>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <table class="table table-striped table-hover">
      <thead>
      <tr>
        <th>序号</th>
        <th>分册书名</th>
        <th>类型</th>
        <th>附件名称</th>
        <th class="text-center">操作</th>
      </tr>
      </thead>
      <tbody>
      <% for(var i = 0; i < info.bookFascicleEntities.length; i++) { %>
      <tr data-type="ebook" data-id="<%- info.bookFascicleEntities[i].bookTopicId %>" data-bookFileId="<%- info.bookFascicleEntities[i].bookFileId %>" data-bookFileName="<%- info.bookFascicleEntities[i].bookFileName %>">
        <td><%- (i + 1)  %></td>
        <td><%- info.bookFascicleEntities[i].bookFascicleBookName %></td>
        <td>
          <% if(info.bookFascicleEntities[i].bookFileType==='1'){ %>
          汉文书稿
          <% } else if(info.bookFascicleEntities[i].bookFileType==='2'){ %>
          盲文书稿
          <% } else if(info.bookFascicleEntities[i].bookFileType==='3'){ %>
          盲封一
          <% } else if(info.bookFascicleEntities[i].bookFileType==='4'){ %>
          盲封二
          <% } else if(info.bookFascicleEntities[i].bookFileType==='5'){ %>
          汉封一
          <% } else if(info.bookFascicleEntities[i].bookFileType==='6'){ %>
          汉封二
          <% } else if(info.bookFascicleEntities[i].bookFileType==='7'){ %>
          汉封四
          <% } else if(info.bookFascicleEntities[i].bookFileType==='8'){ %>
          条形码
          <% } %>
        </td>
        <td class="bookName"><%- info.bookFascicleEntities[i].bookFileName %></td>
        <td>
          <div class="col-sm-12" >
            <div class="col-sm-2" >
              <button data-download="<%- info.bookFascicleEntities[i].bookFileAddress %>" class="btn btn-primary btn-sm btn-download">下载</button>
            </div>
            <div class="col-sm-10" >
              <div class="input-group">
                <input type="text" class="form-control bookAddress" value="<%- info.bookFileAddress %>"
                       data-id="<%- info.bookFascicleEntities[i].bookFileId %>"  readonly />
                <span class="input-group-btn">
                  <button class="btn btn-default" type="button" data-open="public/file/file-upload"
                          data-data="{}"><i class="fa fa-file-o"></i></button>
                </span>
              </div>
            </div>
          </div>
        </td>
      </tr>
      <% } %>
      </tbody>
    </table>
  </div>
  <% if(info.userTaskEntities) { %>
  <% for(var i = 0; i < info.userTaskEntities.length; i++) { %>
  <div class="row" data-flow="<%- info.userTaskEntities[i].flowId %>" data-area="nextFlow">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">下一流程</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.userTaskEntities[i].flowName %></p>
          <input type="hidden" name="flowId" value="<%- info.userTaskEntities[i].flowId %>">
          <input type="hidden" name="flowName" value="<%- info.userTaskEntities[i].flowName %>">
          <input type="hidden" name="nextCategory" value="<%- info.userTaskEntities[i].category %>">
          <input type="hidden" name="deptIds" value="<%- info.userTaskEntities[i].deptIds %>">
          <input type="hidden" name="roleIds" value="<%- info.userTaskEntities[i].roleIds %>">
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <% if(info.userTaskEntities[i].roleIds) { %>
      <div class="form-group">
        <label class="col-xs-4 control-label">任务人<span class="must">*</span></label>
        <div class="col-xs-8">
          <div class="input-group">
            <input type="text" class="form-control" name="taskUsers"
                   value="<%- info.userTaskEntities[i].nextFlowTaskUserNames %>"
                   data-value="<%- info.userTaskEntities[i].nextFlowTaskUserIds %>" placeholder="任务人" readonly
                   data-data="{roleIds: '<%- info.userTaskEntities[i].roleIds %>', searchDepartmentId: '<%- info.userTaskEntities[i].deptIds %>'}" data-fv-notempty="true" data-fv-notempty-message="请选择下一任务人">
            <span class="input-group-btn">
              <button class="btn btn-default" type="button" data-open="public/user/select-users"
                      data-data='{"roleIds": "<%- info.userTaskEntities[i].roleIds %>", "deptIds": "<%- info.userTaskEntities[i].deptIds %>"}'><i class="fa fa-user-o"></i>
              </button>
            </span>
          </div>
        </div>
      </div>
      <% } %>
    </div>
  </div>
  <% } %>
  <% } %>
</div>
<% }else{ %>
<div class="form-horizontal" >
  <div class="row">
    <table class="table table-striped table-hover">
      <thead>
      <tr>
        <th>序号</th>
        <th>分册书名</th>
        <th>类型</th>
        <th>附件名称</th>
        <th>操作</th>
      </tr>
      </thead>
      <tbody>
      <% for(var i = 0; i < info.bookFascicleEntities.length; i++) { %>
      <tr data-id="<%- info.bookFascicleEntities[i].bookTopicId %>" data-name="<%- info.bookFascicleEntities[i].bookTopicName %>">
        <td><%- (i + 1)  %></td>
        <td><%- info.bookFascicleEntities[i].bookFascicleBookName %></td>
        <td>
          <% if(info.bookFascicleEntities[i].bookFileType==='1'){ %>
          汉文书稿
          <% } else if(info.bookFascicleEntities[i].bookFileType==='2'){ %>
          盲文书稿
          <% } else if(info.bookFascicleEntities[i].bookFileType==='3'){ %>
          盲封一
          <% } else if(info.bookFascicleEntities[i].bookFileType==='4'){ %>
          盲封二
          <% } else if(info.bookFascicleEntities[i].bookFileType==='5'){ %>
          汉封一
          <% } else if(info.bookFascicleEntities[i].bookFileType==='6'){ %>
          汉封二
          <% } else if(info.bookFascicleEntities[i].bookFileType==='7'){ %>
          汉封四
          <% } else if(info.bookFascicleEntities[i].bookFileType==='8'){ %>
          条形码
          <% } %>
        </td>
        <td><%- info.bookFascicleEntities[i].bookFileName %></td>
        <td>
          <button class="btn btn-primary btn-sm">下载</button>
        </td>
      </tr>
      <% } %>
      </tbody>
    </table>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">提交人</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.submitUserName %></p>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">提交时间</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.submitDatetime %></p>
        </div>
      </div>
    </div>
  </div>
</div>
<% } %>

<!-- 印前质检 -->
<div class="row">
  <div class="col-xs-3">
    <!-- 左侧树 -->
    <div class="panel">
      <div class="panel-header custom-panel-header">
        <h3><i class="fa fa-book"></i>印前质检</h3>
      </div>
      <div class="panel-content panel-height-1">
        <div id="monthTree">

        </div>
      </div>
    </div>
  </div>
  <div class="col-xs-9">
    <div class="row">
      <div class="col-xs-12">
        <div class="panel">
          <div class="panel-header custom-panel-header">
            <h3>印前质检
              <span class="icheck-inline task-about" id="taskAboutArea"></span>
            </h3>
            <div class="control-btn">
              <button class="btn btn-xs btn-blue" id="btnDeal" data-auth="0341" disabled>处理</button>
              <button class="btn btn-xs btn-default" id="btnView" data-auth="0342" disabled>查看</button>
            </div>
          </div>
          <div class="panel-content">
            <div class="table-10">
              <table class="table table-striped table-hover" data-multi="true" id="workListTable">
                <thead>
                <tr>
                  <th>序号</th>
                  <th><i class="fa fa-square-o" data-action="check"></i></th>
                  <th>图书名称</th>
                  <th>图书册数</th>
                  <th>抽检页</th>
                  <th>图书版数</th>
                  <th>差错率‰</th>
                  <th>状态</th>
                </tr>
                </thead>
                <tbody id="workListTBody">

                </tbody>
              </table>
            </div>
            <div class="pagination-container" id="workListPaging">

            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="panel">
          <div class="panel-header custom-panel-header">
            <h3>流程查看</h3>
          </div>
          <div class="panel-content">
            <div class="table-5-static">
              <table class="table table-striped table-hover">
                <thead>
                <tr>
                  <th>序号</th>
                  <th>状态</th>
                  <th>负责人</th>
                  <th>起始时间</th>
                  <th>完成时间</th>
                  <th>审核结果</th>
                  <th>详细信息</th>
                </tr>
                </thead>
                <tbody id="taskWorkflowTable">

                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!--译校通读众包管理-->
<div class="row">
  <div class="col-xs-12">
    <div class="panel">
      <div class="panel-header custom-panel-header">
        <h3>
          <i class="fa fa-braille"></i>盲文读物管理 - 译校通读众包管理- 任务列表
          <span class="icheck-inline task-about">[ <label><input type="checkbox" id="aboutMe" checked>可领用</label> ]</span>
        </h3>
      </div>
      <div class="panel-content border-bottom">
        <div class="form-horizontal">
          <div class="row">
            <div class="col-xs-5">
              <div class="form-group">
                <label class="col-xs-4 control-label">图书名称</label>
                <div class="col-xs-8">
                  <input type="text" maxlength="32" class="form-control" name="searchBookName"/>
                </div>
              </div>
            </div>
            <div class="col-xs-7">
              <button class="btn btn-primary" id="btnSearchWork">查询</button>
              <button style="float: right;" class="btn btn-primary" data-auth="0321" id="btnCrowdSourceGet" disabled>领取</button>
            </div>
          </div>
        </div>
      </div>
      <div class="panel-content">
        <div class="table-10">
          <table class="table table-striped table-nowrap table-hover" id="crowdSourceTable">
            <thead>
            <tr>
              <th class="w-table">序号</th>
              <th class="w-table"></th><!--<i class="fa fa-square-o" data-action="check"></i>      没有复选-->
              <th>图书名称</th>
              <th>任务阶段</th>
              <th>责任编辑</th>
              <th>发布人</th>
              <th>领用人</th>
              <th>剩余数量/总数</th>
              <th>发布时间</th>
              <th>任务简介</th>
            </tr>
            </thead>
            <tbody id="crowdSourceList"></tbody>
          </table>
        </div>
        <div class="clearfix" id="crowdSourceListPaging"></div>
      </div>
    </div>
  </div>
</div>
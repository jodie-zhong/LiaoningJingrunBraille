<!--劳务管理-->
<div class="row">
  <div class="col-xs-3">
    <div class="panel panel-tree-2">
      <div class="panel-header custom-panel-header">
        <h3 class="modules-name" title="汉文读物管理 - 劳务管理"><i class="fa fa-language"></i> 汉文读物管理 - 劳务管理</h3>
      </div>
      <div class="panel-content">
        <div style="height: 60px;text-align: center">
            <label class="radio-inline p-r-20"><input type="radio" name="handleState" value="1" checked/>社内</label>
            <label class="radio-inline"><input type="radio" name="handleState" value="2"/>社外</label>
        </div>
        <div id="monthTree">
        </div>
      </div>
    </div>
  </div>
  <div class="col-xs-9">
    <div class="row">
      <div class="col-xs-12">
        <div class="panel panel-book-0">
          <div class="panel-header custom-panel-header">
            <h3>流程查看</h3>
            <div class="control-btn">
              <button class="btn btn-xs btn-primary" data-auth="" id="btnLeadIn" >导入</button>
              <button class="btn btn-xs btn-primary" data-auth="" id="btnEntering">录入</button>
            </div>
          </div>
            <div class="panel-content">
                <div class="table-10">
                    <table class="table table-striped table-nowrap table-hover" id="jobIn">
                        <thead>
                        <tr>
                            <th class="w-table">序号</th>
                            <th>姓名</th>
                            <th>出勤天数</th>
                            <th>全勤奖</th>
                            <th>加班费</th>
                            <th>劳务费</th>
                            <th>效益津贴</th>
                            <th>伙食补贴</th>
                            <th>郊区补贴</th>
                            <th>合计</th>
                        </tr>
                        </thead>
                        <tbody id="jobListIn"></tbody>
                    </table>
                    <table class="table table-striped table-nowrap table-hover" id="jobOut" style="display: none">
                        <thead>
                        <tr>
                            <th class="w-table">序号</th>
                            <th>姓名</th>
                            <th>身份证号码</th>
                            <th>金额</th>
                            <th>税额</th>
                            <th>实付金额</th>
                        </tr>
                        </thead>
                        <tbody id="jobListOut"></tbody>
                    </table>
                </div>
                <div id="jobListPaging"></div>
            </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="fascicle" data-area="changeRecordItem">
	<div class="row">
		<div class="col-xs-12">
			<div class="form-group">
				<label class="col-xs-2 control-label">变更名称</label>
				<div class="col-xs-4">
					<p class="form-control-static" data-value="change" data-name="<%- info.name %>"><%- info.title %></p>
					<input type="hidden" data-name="name" name="<%- info.name %>" value="<%- info.name %>" />
				</div>
				<div class="col-xs-5"></div>
				<div class="col-xs-1">
					<button class="btn btn-xs btn-danger" data-type="change-minus"><i class="fa fa-close box"></i></button>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-xs-12">
			<div class="form-group ">
				<label class="col-xs-2 control-label">变更前</label>
				<div class="col-xs-10">
					<p class="form-control-static" data-value="before"><%- info.oldValue %></p>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-xs-12">
			<div class="form-group ">
				<label class="col-xs-2 control-label">变更后</label>
				<%= info.html %>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-xs-12">
			<div class="form-group">
				<label class="col-xs-2 control-label">变更原因</label>
				<div class="col-xs-10">
					<textarea class="form-control" name="changeFileCause" placeholder="变更原因（不超过256字）" maxlength="256"><%- info.changeFileCause %></textarea>
				</div>
			</div>
		</div>
	</div>
</div>
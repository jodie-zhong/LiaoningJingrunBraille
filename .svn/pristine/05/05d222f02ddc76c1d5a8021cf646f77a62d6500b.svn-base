<!--印前质检-质检修改-->
<!---------------------------------------------------------------------------------------------编辑状态--------------------------------------------------------------------------------------------->
<% if(isEdit) { %>
<div class="form-horizontal">
  <div class="row">
    <table class="table table-striped table-hover">
      <thead>
      <tr>
        <th>序号</th>
        <th>类型</th>
        <th>名称</th>
        <th>操作</th>
      </tr>
      </thead>
      <tbody>
      <% if(info.clearDetailOneEntities) { %>
      <% for(var i = 0; i < info.clearDetailOneEntities.length; i++) { %>
      <tr>
        <td><%- (i + 1) %></td>
        <td><%- info.clearDetailOneEntities[i].bookFileType %></td>
        <td><%- info.clearDetailOneEntities[i].bookFileName %></td>
        <td>
          <div class="input-group">
            <input type="hidden" minlength="1" class="form-control" name="uploadFile" value="<%- info.clearDetailOneEntities[i].bookFileName %>"
                   data-value="<%- info.clearDetailOneEntities[i].bookFilePath %>" data-id="<%- info.clearDetailOneEntities[i].bookFilePath %>" placeholder="文件路径" readonly>
            <span class="input-group-btn"><button class="btn btn-default" type="button" data-open="public/file/file-upload" data-data="{}">上传</button></span>
          </div>
        </td>
      </tr>
      <% } %>
      <% } %>
      </tbody>
    </table>
  </div>
  <div class="row">
    <label>版数录入</label>
  </div>
  <div class="row">
    <table class="table table-striped table-hover">
      <thead>
      <tr>
        <th>序号</th>
        <th>册数</th>
        <th>版数</th>
        <th>盲文文稿</th>
      </tr>
      </thead>
      <tbody>
      <% for(var i = 0; i < info.userTaskEntities.length; i++) { %>
      <tr>
        <td><%- (i + 1) %></td>
        <td><%- info.userTaskEntities[i].ceshu %></td>
        <td><input type="text" class="form-control" value="<%- info.userTaskEntities[i].banshu %>" name="banshu" placeholder="盲文版数" maxlength="10" data-fv-field="banshu"></td>
        <td>
          <div class="input-group">
            <input type="hidden" minlength="1" class="form-control" name="uploadFile" value="<%- info.clearDetailOneEntities[i].bookFileName %>"
                   data-value="<%- info.clearDetailOneEntities[i].bookFilePath %>" data-id="<%- info.clearDetailOneEntities[i].bookFilePath %>" placeholder="文件路径" readonly>
            <span class="input-group-btn"><button class="btn btn-default" type="button" data-open="public/file/file-upload" data-data="{}">上传</button></span>
          </div>
        </td>
      </tr>
      <% } %>
      </tbody>
    </table>
  </div>
  <% if(info.userTaskEntities) { %>
  <% for(var i = 0; i < info.userTaskEntities.length; i++) { %>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">下一流程</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.userTaskEntities[i].flowName %></p>
          <input type="hidden" name="flowId" value="<%- info.userTaskEntities[i].flowId %>">
          <input type="hidden" name="flowName" value="<%- info.userTaskEntities[i].flowName %>">
          <input type="hidden" name="nextCategory" value="<%- info.userTaskEntities[i].category %>">
          <input type="hidden" name="deptIds" value="<%- info.userTaskEntities[i].deptIds %>">
          <input type="hidden" name="roleIds" value="<%- info.userTaskEntities[i].roleIds %>">
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <% if(info.userTaskEntities[i].roleIds) { %>
      <div class="form-group">
        <label class="col-xs-4 control-label">任务人<span class="must">*</span></label>
        <div class="col-xs-8">
          <div class="input-group">
            <input type="text" class="form-control" name="taskUsers" value="<%- info.userTaskEntities[i].nextFlowTaskUserNames %>" data-value="<%- info.userTaskEntities[i].nextFlowTaskUserIds %>"
                   placeholder="任务人" data-fv-notempty="true" data-fv-notempty-message="请选择下一任务人" data-data="{searchRoleId: '', searchDepartmentId: ''}" readonly>
            <span class="input-group-btn">
              <button class="btn btn-default" type="button" data-open="public/user/select-users" data-data="{}"><i class="fa fa-user-o"></i></button>
            </span>
          </div>
        </div>
      </div>
      <% } %>
    </div>
  </div>
  <% } %>
  <% } %>
</div>
<!---------------------------------------------------------------------------------------------查看状态--------------------------------------------------------------------------------------------->
<% } else { %>
<div class="form-horizontal">
  <div class="row">
    <table class="table table-striped table-hover">
      <thead>
      <tr>
        <th>序号</th>
        <th>类型</th>
        <th>名称</th>
        <th>操作</th>
      </tr>
      </thead>
      <tbody>
      <% if(info.clearDetailOneEntities) { %>
      <% for(var i = 0; i < info.clearDetailOneEntities.length; i++) { %>
      <tr>
        <td><%- (i + 1) %></td>
        <td><%- info.clearDetailOneEntities[i].bookFileType %></td>
        <td><%- info.clearDetailOneEntities[i].bookFileName %></td>
        <td><a class="btn btn-xs btn-default downloadSeparated" data-download-address="<%- info.userTaskEntities[i].bookFileAddress %>">下载</a></td>
      </tr>
      <% } %>
      <% } %>
      </tbody>
    </table>
  </div>
  <div class="row">
    <label>版数录入</label>
  </div>
  <div class="row">
    <table class="table table-striped table-hover">
      <thead>
      <tr>
        <th>序号</th>
        <th>册数</th>
        <th>版数</th>
        <th>盲文文稿</th>
      </tr>
      </thead>
      <tbody>
      <% for(var i = 0; i < info.userTaskEntities.length; i++) { %>
      <tr>
        <td><%- (i + 1) %></td>
        <td><%- info.userTaskEntities[i].ceshu %></td>
        <td><%- info.userTaskEntities[i].banshu %></td>
        <td><a class="btn btn-xs btn-default downloadSeparated" data-download-address="<%- info.userTaskEntities[i].bookFileAddress %>">下载</a></td>
      </tr>
      <% } %>
      </tbody>
    </table>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">提交人</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.submitUserName %></p>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">提交时间</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.submitDatetime %></p>
        </div>
      </div>
    </div>
  </div>
</div>
<% } %>
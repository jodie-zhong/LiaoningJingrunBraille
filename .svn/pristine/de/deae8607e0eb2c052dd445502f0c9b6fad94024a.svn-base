<!--刻印/制版-->
<% if(isEdit) { %>
<div class="form-horizontal">
	<div class="job" id="addJob">
		<div class="row">
			<div class="col-xs-6">
				<div class="form-group">
					<label class="col-xs-4 control-label">工作日期</label>
					<div class="col-xs-8">
						<div class="input-group">
							<input type="text" class="form-control" name="bookTopicName" value="<%- info.bookTopicName %>" maxlength="128" placeholder="工作日期" />
							<div class="help-block with-errors"></div>
						</div>
					</div>
				</div>
			</div>
			<div class="col-xs-6">
				<div class="input-group">
					<button class="btn btn-default addjob" type="button"><i class="fa fa-plus-circle"></i></button>
					<button class="btn btn-default" type="button" style="display: none;"><i class="fa fa-close"></i></button>
				</div>
			</div>
		</div>
		<div class="row">
			<table class="table table-striped table-hover">
				<thead>
					<tr>
						<th style="width: 50px;">序号</th>
						<th style="width: 170px;">机器型号</th>
						<th style="width: 90px;">开数</th>
						<th style="width: 90px;">版数</th>
						<th style="width: 90px;">张数</th>
						<th style="width: 90px;">差错率</th>
						<th style="width: 90px;"></th>
						<th style="width: 90px;"></th>
					</tr>
				</thead>
				<tbody id="addrow">
					<tr>
						<td></td>
						<td>
							<select class="form-control">
								<option>1台机器(美国)</option>
								<option>1台机器(比利时)</option>
								<option>1台机器</option>
							</select>
						</td>
						<td><input type="text" style="width: 100%;" /></td>
						<td><input type="text" style="width: 100%;" /></td>
						<td><input type="text" style="width: 100%;" /></td>
						<td><input type="text" style="width: 100%;" /></td>
						<td><button class="btn btn-default addRow"><i class="fa fa-plus-circle"></i></button></td>
						<td><button class="btn btn-default delRow" style="display: none;"><i class="fa fa-close"></i></button></td>
					</tr>
				</tbody>
			</table>
		</div>
	</div>
	<% if(info.userTaskEntities) { %>

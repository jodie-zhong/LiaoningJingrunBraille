<% for(var i = 0; i < list.length; i++) { %>
<tr data-id="<%- list[i].departmentId %>">
  <td><%- (i + 1) %></td>
  <!--<td><input type="checkbox"></td>-->
  <td><%- list[i].departmentName %></td>
  <td><%- list[i].departmentUserNum %></td>
  <td><%- list[i].departmentPhone %></td>
  <td><%- list[i].departmentPrincipalName %></td>
  <td><%- list[i].departmentLeaderName %></td>
</tr>
<% } %>
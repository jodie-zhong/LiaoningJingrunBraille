<% for(var i = 0; i < roll.length; i++) { %>
<tr data-id="<%- roll[i].userId %>">
  <td style="width: 50px;"><%- (i + 1) %></td>
  <td style="width: 20%;"><%- roll[i].userName %></td>
  <td style="width: 20%;"><%- roll[i].userPhone %></td>
  <td style="width: 20%;"><%- roll[i].departmentName %></td>
  <td style="width: 20%;"><%- roll[i].roleNames %></td>
  <td style="width: 80px;"><%- roll[i].userIntegral %></td>
</tr>
<% } %>
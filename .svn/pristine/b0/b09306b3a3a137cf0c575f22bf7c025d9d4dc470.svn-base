<!-- 合同录入 -->
<div class="row">
	<!-- 左侧树 -->
	<div class="col-xs-3">
		<div class="panel">
			<div class="panel-header custom-panel-header">
				<h3 class="modules-name" title="汉文读物管理- 合同录入"><i class="fa fa-language"></i> 汉文读物管理- 合同录入</h3>
			</div>
			<div class="panel-content">
				<div id="monthTree"></div>
			</div>
		</div>
	</div>
	<div class="col-xs-9">
		<div class="row">
			<div class="col-xs-12">
				<div class="panel">
					<div class="panel-header custom-panel-header">
						<h3>
							合同录入
							<span class="icheck-inline task-about" id="taskAboutArea"></span>
						</h3>
						<div class="control-btn">
							<button class="btn btn-xs btn-primary" id="btnInputHandle" data-auth="" disabled>处理</button>
							<button class="btn btn-xs btn-default" id="btnInputCheck" data-auth="" disabled>查看</button>
						</div>
					</div>
					<div class="panel-content">
						<div class="table-10 table-scroll">
							<table class="table table-striped table-hover table-nowrap" data-multi="true" id="workListTable">
								<thead>
									<tr>
										<th style="width: 50px;">序号</th>
										<th style="width: 50px;"><i class="fa fa-square-o" data-action="check"></i></th>
										<th>图书名称</th>
										<th>图书类型</th>
										<th>处理人</th>
										<th>合同状态</th>
									</tr>
								</thead>
								<tbody id="workListTable"></tbody>
							</table>
						</div>
						<div class="pagination-container" id="workListPaging"></div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
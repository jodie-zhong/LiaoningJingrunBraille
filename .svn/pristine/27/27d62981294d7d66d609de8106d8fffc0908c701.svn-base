<!--选题备案登记表新增-->
<% if(isEdit) { %>
<div class="form-horizontal">
  <div class="row">
  	<div class="col-xs-12">
  		<div class="form-group">
  			<label class="col-xs-2 control-label">备案选题</label>
  			<div class="col-xs-4">
  				<div class="input-group">
  					<input type="text" class="form-control" value="<%- info.a %>" name="a" placeholder="备案选题" />
  					<span class="input-group-btn">
  						<button class="btn btn-default" type="button" data-open = "public/topic/select-topic" data-data = "{}"><i class="fa fa-file-o"></i></button>
  					</span>
  				</div>
  			</div>
  		</div>
  	</div>
  </div>
  <div class="row">
    <div class="form-group col-xs-12">
      <label class="col-xs-2 control-label">备案方式</label>
      <div class="col-xs-10">
        <label class="radio-inline">
          <% if(info.handleState === '1') { %>
          <input type="radio" name="handleState" value="1" checked>重大选题备案
          <% } else { %>
          <input type="radio" name="handleState" value="1">重大选题备案
          <% } %>
        </label>
        <label class="radio-inline">
          <% if(info.handleState === '2') { %>
          <input type="radio" name="handleState" value="2" checked>引用选题备案
          <% } else { %>
          <input type="radio" name="handleState" value="2">引用选题备案
          <% } %>
        </label>
      </div>
    </div>
  </div>
  <div data-area="import" style="display: none;">
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">选题名称</label>
        <div class="col-xs-8">
          <div class="input-group">
            <input type="text" class="form-control" name="importantTopicFileName" value="<%- info.importantTopicFileName %>"
                   maxlength="32" placeholder="选题名称" readonly/>
          </div>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">著作责任者</label>
        <div class="col-xs-8">
          <input type="text" class="form-control" value="<%- info.importantTopicFileAuthor %>" name="importantTopicFileAuthor"
                 placeholder="著作责任者" maxlength="32"/>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">字/幅数（千字/幅）</label>
        <div class="col-xs-8">
          <input type="number" class="form-control" value="<%- info.importantTopicFileWords %>" name="importantTopicFileWords"
                 placeholder="字/幅数（千字/幅）" maxlength="32"/>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">拟出版时间</label>
        <div class="col-xs-8">
          <input type="text" class="form-control date-time" value="<%- info.planPublicationTime %>" name="planPublicationTime"
                 placeholder="拟出版时间" maxlength="32"/>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">涉及重大选题类别</label>
        <div class="col-xs-8">
          <input type="text" class="form-control" value="<%- info.bookDetailAuthorEducation %>"
                 name="bookDetailAuthorEducation" placeholder="涉及重大选题类别" maxlength="32"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group ">
        <label class="col-xs-4 control-label">责任编辑</label>
        <div class="col-xs-8">
          <input type="number" class="form-control" value="<%- info.bookDetailWords %>" name="bookDetailWords"
                 placeholder="责任编辑" maxlength="10"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <div class="form-group">
        <label class="col-xs-2 control-label">著作责任者简介</label>
        <div class="col-xs-10">
          <textarea class="form-control" name="bookDetailAuthorResult" placeholder="著作责任者简介" maxlength="256"><%- info.bookDetailAuthorResult %></textarea>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <div class="form-group">
        <label class="col-xs-2 control-label">书稿内容简介</label>
        <div class="col-xs-10">
        <textarea class="form-control"
                  name="bookDetailAuthorContactWay" placeholder="书稿内容简介" maxlength="128"><%- info.bookDetailAuthorContactWay %></textarea>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <div class="form-group">
        <label class="col-xs-2 control-label">拟审核问题</label>
        <div class="col-xs-10">
          <textarea class="form-control" name="bookDetailSketched" placeholder="拟审核问题" maxlength="1024"><%- info.bookDetailSketched %></textarea>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
  </div>
  </div>
  <div data-area = "cite" style="display: none;">
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">作品名称（原文）</label>
        <div class="col-xs-8">
          <div class="input-group">
            <input type="text" class="form-control" name="bookTopicName" value="<%- info.bookTopicName %>"
                   maxlength="128" placeholder="作品名称"/>
            <div class="help-block with-errors"></div>
          </div>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">作品翻译名称</label>
        <div class="col-xs-8">
          <input type="text" class="form-control" value="<%- info.bookSeriesName %>" name="bookSeriesName"
                 placeholder="作品翻译名称" maxlength="128"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">拟中文出版名称</label>
        <div class="col-xs-8">
          <div class="input-group">
            <input type="text" class="form-control" name="bookTopicName" value="<%- info.bookTopicName %>"
                   maxlength="128" placeholder="拟中文出版名称"/>
            <div class="help-block with-errors"></div>
          </div>
        </div>
      </div>
    </div>
    <div class="col-xs-6"></div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">作者</label>
        <div class="col-xs-8">
          <input type="text" class="form-control" value="<%- info.bookDetailAuthor %>" name="bookDetailAuthor"
                 placeholder="作者" maxlength="32"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">国籍</label>
        <div class="col-xs-8">
          <input type="text" class="form-control" value="<%- info.bookDetailWorkWay %>" name="bookDetailWorkWay"
                 placeholder="国籍" maxlength="64"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">著作权人</label>
        <div class="col-xs-8">
          <input type="text" class="form-control" value="<%- info.bookDetailAuthorEducation %>"
                 name="bookDetailAuthorEducation" placeholder="著作权人" maxlength="32"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group ">
        <label class="col-xs-4 control-label">国籍</label>
        <div class="col-xs-8">
          <input type="number" class="form-control" value="<%- info.bookDetailWords %>" name="bookDetailWords"
                 placeholder="国籍" maxlength="10"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">著作权持有者</label>
        <div class="col-xs-8">
          <input type="text" class="form-control" value="<%- info.bookDetailAuthorTitles %>"
                 name="bookDetailAuthorTitles" placeholder="著作权持有者" maxlength="32"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group ">
        <label class="col-xs-4 control-label">国籍</label>
        <div class="col-xs-8">
          <input type="text" class="form-control date-time" value="<%- info.bookDetailSubmitDatetime %>"
                 name="bookDetailSubmitDatetime" placeholder="国籍"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">原作品出版日期</label>
        <div class="col-xs-8">
          <input type="text" class="form-control" value="<%- info.bookDetailName %>" name="bookDetailName"
                 placeholder="原书名称" required data-required-error="原作品出版日期" maxlength="128"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">合同签到日期及有效日期</label>
        <div class="col-xs-8">
          <input type="number" class="form-control" value="<%- info.bookDetailEdition %>" name="bookDetailEdition"
                 placeholder="合同签到日期及有效日期" maxlength="10"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">授权内容</label>
        <div class="col-xs-8">
          <input type="text" class="form-control" value="<%- info.bookDetailPublishingHouse %>"
                 name="bookDetailPublishingHouse" placeholder="授权内容" maxlength="128"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">发行范围</label>
        <div class="col-xs-8">
          <input type="text" class="form-control date-time" value="<%- info.bookDetailPublicationDate %>"
                 name="bookDetailPublicationDate" placeholder="发行范围"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">付酬方式</label>
        <div class="col-xs-8">
          <input type="text" class="form-control" value="<%- info.bookDetailPublishingHouse %>"
                 name="bookDetailPublishingHouse" placeholder="付酬方式" maxlength="128"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">付酬标准</label>
        <div class="col-xs-8">
          <input type="text" class="form-control date-time" value="<%- info.bookDetailPublicationDate %>"
                 name="bookDetailPublicationDate" placeholder="付酬标准"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">支持币种</label>
        <div class="col-xs-8">
          <input type="text" class="form-control" value="<%- info.bookDetailPublishingHouse %>"
                 name="bookDetailPublishingHouse" placeholder="支持币种" maxlength="128"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">最低印数</label>
        <div class="col-xs-8">
          <input type="text" class="form-control date-time" value="<%- info.bookDetailPublicationDate %>"
                 name="bookDetailPublicationDate" placeholder="最低印数"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">版权联系人</label>
        <div class="col-xs-8">
          <input type="text" class="form-control" value="<%- info.bookDetailPublishingHouse %>"
                 name="bookDetailPublishingHouse" placeholder="版权联系人" maxlength="128"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">责任编辑</label>
        <div class="col-xs-8">
          <input type="text" class="form-control date-time" value="<%- info.bookDetailPublicationDate %>"
                 name="bookDetailPublicationDate" placeholder="责任编辑"/>
          <div class="help-block with-errors"></div>
        </div>
      </div>
    </div>
  </div>
  </div>
  <% if(info.userTaskEntities) { %>
  <% for(var i = 0; i < info.userTaskEntities.length; i++) { %>
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">下一流程</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.userTaskEntities[i].userTaskName %></p>
          <input type="hidden" name="userTaskId" value="<%- info.userTaskEntities[i].userTaskId %>">
          <input type="hidden" name="userTaskName" value="<%- info.userTaskEntities[i].userTaskName %>">
          <input type="hidden" name="nextCategory" value="<%- info.userTaskEntities[i].category %>">
          <input type="hidden" name="deptIds" value="<%- info.userTaskEntities[i].deptIds %>">
          <input type="hidden" name="roleIds" value="<%- info.userTaskEntities[i].roleIds %>">
          <input type="hidden" name="taskId" value="<%- info.taskId %>">
        </div>
      </div>
    </div>
    <div class="col-xs-6">
      <% if(info.userTaskEntities[i].roleIds) { %>
      <div class="form-group">
        <label class="col-xs-4 control-label">任务人<span class="c-red">*</span></label>
        <div class="col-xs-8">
          <div class="input-group">
            <input type="text" class="form-control" name="taskUsers"
                   value="<%- info.userTaskEntities[i].nextFlowTaskUserNames %>"
                   data-value="<%- info.userTaskEntities[i].nextFlowTaskUserIds %>" placeholder="任务人" readonly
                   data-data="{searchRoleId: '', searchDepartmentId: ''}" required  data-required-error="请选择任务人">
            <span class="input-group-btn">
              <button class="btn btn-default" type="button" data-open="public/user/select-users" data-data="{}"><i
                      class="fa fa-user-o"></i></button>
            </span>
          </div>
          <div class="help-block with-errors"></div>
        </div>
      </div>
      <% } %>
    </div>
  </div>
  <% } %>
  <% } %>
</div>
<% } else { %>
<div class="form-horizontal">
  <div class="row">
    <div class="col-xs-6">
      <div class="form-group">
        <label class="col-xs-4 control-label">选题名称</label>
        <div class="col-xs-8">
          <p class="form-control-static"><%- info.bookTopicName %></p>

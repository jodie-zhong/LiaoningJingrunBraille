/* 覆盖登录页样式 */
.account2 .account-info .logo {
  background: none;
}

.account2 .form-signin input[type="password"] {
  margin-bottom: 8px;
}

.account2 .form-signin input.captcha {
  width: 135px;
}

.account2 .form-signin img.captcha {
  width: 75px;
  height: 34px;
  float: right;
  cursor: pointer;
}

.account2 .error-message {
  color: #ff0000;
  margin: 0;
}

.page-wrapper {
  min-height: 100%;
}

.widget-info .right .count-title {
  color: #A3A3A3;
  font-size: 16px;
  text-align: left;
  text-transform: uppercase;
}

/* 暂时覆盖对话框禁用页面滚动的功能， */
.custom-dialog {
  width: 500px;
  border-radius: 5px;
}

.modal-open {
  overflow: auto;
}

/* 覆盖菜单样式，支持禁用 */
.dropdown-menu > .disabled > a, .dropdown-menu > .disabled > a:focus, .dropdown-menu > .disabled > a:hover {
  color: #AAA;
}

.topbar .header-left .topnav > ul > li .dropdown-menu > li.disabled > a:hover {
  background: none;
  color: #AAA;
}

/* 覆盖菜单样式 */
.topbar .header-left .topnav > ul > li > a {
  padding: 13px 10px;
}

.topbar .header-left .topnav > ul > li > a:hover,
.topbar .header-left .topnav > ul:not(.nav-icons) > li > a:focus,
.topbar .header-left .topnav > ul:not(.nav-icons) > li.open > a,
.topbar .header-left .topnav > ul:not(.nav-icons) > li.active > a {
  padding: 13px 10px;
}

/* 覆盖菜单头像 */
.topbar .header-right .header-menu #user-header img {
  width: 36px;
  height: 36px;
}

.topbar .header-right .header-menu .username {
  display: inline-block;
  max-width: 110px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

/* 覆盖表单Label样式 */
.form-horizontal .control-label {
  text-align: right;
}

.form-horizontal pre.form-control-static {
  background: none;
  border: none;
  padding-left: 0;
  padding-right: 0;
  font-family: 'Lato', 'Open Sans', Helvetica, sans-serif !important;
  color: #5b5b5b;
  margin-top: 4px;
}

.form-compact .form-group {
  margin-bottom: 0;
}

.form-horizontal .radio, .form-horizontal .checkbox, .form-horizontal .radio-inline, .form-horizontal .checkbox-inline {
  margin-top: 7px;
}

select.form-control {
  height: 34px;
  padding-top: 7px;
}

/* 覆盖主页面大小 */
.main-content .page-content {
  padding: 20px 20px 20px;
}

.main-content {
  overflow: hidden;
  min-height: 500px;
}


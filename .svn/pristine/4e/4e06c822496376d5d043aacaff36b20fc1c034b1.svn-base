/* 带删除按钮的输入框组 */
.input-group-btn:not(.btn-sm):not(.btn-hg):not(.btn-xs):not(.btn-lg) > .btn:not(.btn-sm):not(.btn-hg):not(.btn-xs):not(.btn-lg){
  width: 35px;
}

.input-group.with-clear input{
  padding-right: 24px;
}

.input-group-btn>.btn.btn-clear {
  background-color: transparent;
  position: absolute;
  right: 35px;
  width: 25px!important;
  color: #c14444;
  z-index: 5;
}

/* 覆盖按钮组样式 */
.input-group-btn:last-child > .btn {
  margin-right: 0;
  padding-left: 10px;
  padding-right: 10px;
}

/* 覆盖表单输入项背景色，默认灰色难看 */
.form-control {
  background-color: #FFFFFF;
}

.form-control[disabled], .form-control[readonly], fieldset[disabled] .form-control {
  background-color: #FFFFFF;
}

/* 修正style.css导致图标偏移 */
.main-content .page-content .panel .panel-content i.box {
  padding: 0 !important;
}

/* 覆盖style.css导致的多列偏移 */
.form-horizontal .row .col-md-12 {
  margin-left: 0;
}

.bootstrap-datetimepicker-widget .picker-switch td span, .bootstrap-datetimepicker-widget table td span {
  text-align: center;
}

.bootstrap-datetimepicker-widget .timepicker-picker a.btn {
  height: auto !important;
  margin: 0;
}

textarea {
  resize: vertical;
}

.copyright {
  text-align: center;
}

/*设置一个固定的宽度*/
.w-table{
  width: 50px;
}
.w-50{
  width: 50px;
}
.w-100{
  width: 100px;
}
.w-150{
  width: 150px;
}
.w-200{
  width: 200px;
}
/*公共图书基本信息的img大小设置*/
#bookCover{
  padding-left: 50px;
  width: 150px;
  height: 150px;
}
#bookDetailPanel  p{

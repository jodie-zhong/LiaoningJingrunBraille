<!--译校通读众包管理-->
<div class="row">
    <div class="col-xs-12">
        <div class="panel">
            <div class="panel-header custom-panel-header">
                <h3><i class="fa fa-braille"></i>盲文读物管理 - 译校通读众包管理- 任务列表</h3>
                <div class="control-btn">
                    <button type="button" class="btn btn-primary" data-auth="0321" id="btnCrowdSourceGet" disabled>领取
                    </button>
                </div>
            </div>
            <div class="panel-content">
                <div class="table-10">
                    <table class="table table-striped table-nowrap table-hover" data-multi="true" id="crowdSourceTable">
                        <thead>
                        <tr>
                            <th class="w-table">序号</th>
                            <th class="w-table"><i class="fa fa-square-o" data-action="check"></i></th>
                            <th>图书名称</th>
                            <th>任务阶段</th>
                            <th>责任编辑</th>
                            <th>发布人</th>
                            <th>发布时间</th>
                            <th>任务简介</th>
                        </tr>
                        </thead>
                        <tbody id="crowdSourceList"></tbody>
                    </table>
                </div>
                <div class="clearfix" id="crowdSourceListPaging"></div>
            </div>
        </div>
    </div>
</div>